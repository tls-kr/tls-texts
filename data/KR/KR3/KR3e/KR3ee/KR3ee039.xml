<p xml:id="KR3ee039_jc_000-d3p54">
          <seg xml:id="KR3ee039_jc_000-d3.p54s1">右經由肝經別支接蓄門<c n="，"/>
          </seg>
          <seg xml:id="KR3ee039_jc_000-d3.p54s2">出鼻<c n="，"/>
