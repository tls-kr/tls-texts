<div n="h1" xml:id="KR3eo028_jc_000-d2">
        <head n="h1" xml:id="KR3eo028_jc_000-d2h1">
          <seg xml:id="KR3eo028_jc_000-d2.h1s1">軒轅黃帝治病秘法事例</seg>
        </head>
        <p xml:id="KR3eo028_jc_000-d2p2">
          <seg xml:id="KR3eo028_jc_000-d2.p2s1">凡遇正月元旦<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p2s2">用三牲酒禮祭之<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p2s3">設立子亥二時祭<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p4">
          <seg xml:id="KR3eo028_jc_000-d2.p4s1">軒轅黃帝神位禮拜<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p4s2">弟子某人祭文<c n="："/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p4s3">入意思真慕道想丹華<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p4s4">滌盧澄心<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p4s5">以期輕舉濟人助國<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p4s6">立功為先<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p4s7">凡用所祈<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p4s8">尅獲通感<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p4s9">天上天下咸乞聞知<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p4s10">先用新筆一枝<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p4s11">京墨一塊<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p4s12">向太陽咒曰<c n="："/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p6">
          <seg xml:id="KR3eo028_jc_000-d2.p6s1">赫赫陽陽<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p6s2">日出東方<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p6s3">神筆在手<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p6s4">萬病消亡<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p8">
          <seg xml:id="KR3eo028_jc_000-d2.p8s1">次用好醋研墨<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p8s2">染筆毫上<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p8s3">用筆於香菸頂燻之<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p8s4">咒曰<c n="："/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p10">
          <seg xml:id="KR3eo028_jc_000-d2.p10s1">日出東方毒如蛇<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p10s2">不作膿<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p10s3">不作血<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p10s4">隨著日氣自消滅<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p10s5">急急如律令<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p12">
          <seg xml:id="KR3eo028_jc_000-d2.p12s1">某年<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p12s2">月<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p12s3">日弟子某人稽首頓首仰啟</seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p14">
          <seg xml:id="KR3eo028_jc_000-d2.p14s1">祖師軒轅黃帝聖神<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p14s2">某切為濟人助國<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p14s3">立功為先<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p14s4">以今遵稟<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p16">
          <seg xml:id="KR3eo028_jc_000-d2.p16s1">真科奉行秘書<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p16s2">傳以救人為本<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p16s3">同躋壽域<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p16s4">伏望遙降至真先生氣灌注身心<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p16s5">令某書篆速遂通靈<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p16s6">凡有禱祈<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p16s7">克獲通感<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p16s8">下情不勝皈命虔禱之至<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p18">
          <seg xml:id="KR3eo028_jc_000-d2.p18s1">事例畢<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p20">
          <seg xml:id="KR3eo028_jc_000-d2.p20s1">⿱𫩠⿰飠谷<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p20s2">⿱𫩠⿰飠臣<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p20s3">⿱𫩠⿰飠看<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p20s4">此三字治一切無名腫毒<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p20s5">於疾處書之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p22">
          <seg xml:id="KR3eo028_jc_000-d2.p22s1">⿱𫩠𩚪<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p22s2">此一字治蠍螫<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p22s3">於疾處書之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p24">
          <seg xml:id="KR3eo028_jc_000-d2.p24s1">⿱𫩠⿰飠食<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p24s2">此一字治婦人<c n="、"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p24s3">室女經脈不通<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p24s4">並乳腫癰<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p24s5">服<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p24s6">貼之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p26">
          <seg xml:id="KR3eo028_jc_000-d2.p26s1">⿱𫩠⿰者食<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p26s2">此一字治人身虎咬<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p26s3">書紙上<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p26s4">燒灰<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p26s5">荊芥湯送下<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p28">
          <seg xml:id="KR3eo028_jc_000-d2.p28s1">⿱𫩠⿰雍食<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p28s2">此一字治蛇咬<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p28s3">疼處書之<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p28s4">婦人吹奶<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p28s5">服之<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p28s6">貼之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p30">
          <seg xml:id="KR3eo028_jc_000-d2.p30s1">⿱𫩠⿰扌食<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p30s2">此一字治蛇咬<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p30s3">疼處書之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p32">
          <seg xml:id="KR3eo028_jc_000-d2.p32s1">⿱𫩠⿰嬰食<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p32s2">此一字治狗咬<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p32s3">書紙上<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p32s4">油煎<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p32s5">服之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p34">
          <seg xml:id="KR3eo028_jc_000-d2.p34s1">⿱𫩠𫗓<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p34s2">此一字治肚疼<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p34s3">書紙上<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p34s4">燒灰<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p34s5">服之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p36">
          <seg xml:id="KR3eo028_jc_000-d2.p36s1">⿱𫩠⿰飠系<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p36s2">此一字治婦人吹妳<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p36s3">或燒灰服之<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p36s4">或貼之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p38">
          <seg xml:id="KR3eo028_jc_000-d2.p38s1">⿱𫩠⿰飠眷<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p38s2">此一字治人遍身疼痛<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p38s3">書紙上<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p38s4">燒灰<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p38s5">服之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p40">
          <seg xml:id="KR3eo028_jc_000-d2.p40s1">⿱𫩠⿰飠化<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p40s2">此一字治癰疽<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p40s3">書紙上<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p40s4">燒灰<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p40s5">服之<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p40s6">貼之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p42">
          <seg xml:id="KR3eo028_jc_000-d2.p42s1">⿱𫩠⿰耳食<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p42s2">此一字治辟邪<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p42s3">書紙上<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p42s4">頂帶三日<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p42s5">燒灰服之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p44">
          <seg xml:id="KR3eo028_jc_000-d2.p44s1">⿱𫩠矝<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p44s2">此一字治破傷風<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p44s3">書紙上<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p44s4">燒灰<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p44s5">服之<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p44s6">並貼之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p46">
          <seg xml:id="KR3eo028_jc_000-d2.p46s1">⿱𫩠⿰捂食<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p46s2">此一字治火燒瘡<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p46s3">服<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p46s4">貼之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p48">
          <seg xml:id="KR3eo028_jc_000-d2.p48s1">⿱𫩠𤝰<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p48s2">此一字治虎咬<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p48s3">書紙上<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p48s4">油煎<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p48s5">貼之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p50">
          <seg xml:id="KR3eo028_jc_000-d2.p50s1">⿱𫩠聆<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p50s2">此一字治氣勞<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p50s3">於疼處書之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p52">
          <seg xml:id="KR3eo028_jc_000-d2.p52s1">⿱𫩠掩<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p52s2">此一字治辟邪<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p52s3">書紙上<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p52s4">帶之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p54">
          <seg xml:id="KR3eo028_jc_000-d2.p54s1">⿱𫩠⿰飠食<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p54s2">此一字治心疾<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p54s3">書紙上<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p54s4">燒灰<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p54s5">服之<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p54s6">及治腰脅疼<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p54s7">書之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p56">
          <seg xml:id="KR3eo028_jc_000-d2.p56s1">⿱𫩠⿰者食<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p56s2">此一字治傷熱<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p56s3">書紙上<c n="，"/>
