          <seg xml:id="KR3eo028_jc_000-d2.p56s4">燒灰<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p56s5">服之<c n="。"/>
          </seg>
        </p>
<p xml:id="KR3eo028_jc_000-d2p58">
          <seg xml:id="KR3eo028_jc_000-d2.p58s1">⿱𫩠⿰嬰食<space n="　" quantity="1"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p58s2">此一字治人咬<c n="。"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p58s3">書紙上<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p58s4">燒灰<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p58s5">服之<c n="，"/>
          </seg>
          <seg xml:id="KR3eo028_jc_000-d2.p58s6">貼之<c n="。"/>
