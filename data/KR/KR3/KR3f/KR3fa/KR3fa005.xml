    <text>
        <body>
            <div>
                <p>
                    <pb ed="KX" n="DLJ0031362.1" xml:id="KR3fa005_tls_1a"/>
                    <head xml:id="KR3fa005_tls_p1.s1-h">
                        <seg xml:id="KR3fa005_tls_p1.s1" state="locked">
                            <lb n="1" ed="KX"/>
                            <space n="䶻䶻" quantity="2"/>唐步天歌</seg>
                    </head>
                    <seg state="locked" xml:id="KR3fa005_tls_p1.s2.1">
                        <lb n="2" ed="KX"/>蓺文志王希明丹元子步天歌一卷中興書目步天歌一卷</seg>
                    <seg state="locked" xml:id="KR3fa005_tls_p1.s2.2" type="comm">崇<lb type="/"/>文</seg>
                    <seg state="locked" xml:id="KR3fa005_tls_p1.s3.1" type="comm">
                        <lb n="3" ed="KX"/>目<lb type="/"/>同</seg>
                    <seg state="locked" xml:id="KR3fa005_tls_p1.s3.2">題右拾遺王希明撰圖二十八宿及太㣲紫㣲天市垣各總為</seg>
                    <seg xml:id="KR3fa005_tls_p1.s4" state="locked">
                        <lb n="4" ed="KX"/>之歌<space n="䶻" quantity="1"/>通志天文畧歴世天文志徒有其書無載象之義學者但</seg>
                    <seg xml:id="KR3fa005_tls_p1.s5" state="locked">
                        <lb n="5" ed="KX"/>識星名不可以仰觀隋有丹元子隐者之流也不知名氏作步天</seg>
                    <seg xml:id="KR3fa005_tls_p1.s6" state="locked">
                        <lb n="6" ed="KX"/>歌見者可以觀象焉王希明纂漢𣈆志以釋之句中有圖言下有</seg>
                    <seg xml:id="KR3fa005_tls_p1.s7" state="locked">
                        <lb n="7" ed="KX"/>象又不言休祥是深知天者歌前亦有星形<space n="䶻" quantity="1"/>晁氏志步天歌一</seg>
                    <seg state="locked" xml:id="KR3fa005_tls_p1.s8.1">
                        <lb n="8" ed="KX"/>卷二十八舍歌也三垣頌五星凌犯賦附於後</seg>
                    <seg state="locked" xml:id="KR3fa005_tls_p1.s8.2" type="comm">缺</seg>
                    <seg state="locked" xml:id="KR3fa005_tls_p1.s8.3">或云唐王希明</seg>
                    <seg xml:id="KR3fa005_tls_p1.s9" state="locked">
                        <lb n="9" ed="KX"/>自號丹元子</seg>
                    <seg xml:id="KR3fa005_tls_p1.s11" state="locked">
                        <lb n="11" ed="KX"/>
                        <space n="䶻䶻" quantity="2"/>東方角</seg>
                    <seg xml:id="KR3fa005_tls_p1.s12" state="locked">
                        <lb n="12" ed="KX"/>兩星南北正直著中有平道上天田總是黑星兩相</seg>
                    <seg xml:id="KR3fa005_tls_p1.s13" state="locked">
                        <lb n="13" ed="KX"/>連別有一烏名進賢平道右畔獨淵然最上三星周</seg>
                    <seg xml:id="KR3fa005_tls_p1.s14" state="locked">
                        <lb n="14" ed="KX"/>𪔂形角下天門左平星䨇𩀱横於庫樓上庫樓十星</seg>
                    <seg xml:id="KR3fa005_tls_p1.s15" state="locked">
                        <lb n="15" ed="KX"/>屈曲明樓中柱有十五星三三相著如𪔂形其中四</seg>
                    <seg xml:id="KR3fa005_tls_p1.s16" state="locked">
                        <lb n="16" ed="KX"/>星別名𢖍南門樓外兩星横</seg>
                    <pb ed="KX" n="DLJ0031362.2" xml:id="KR3fa005_tls_2a"/>
                    <figure>
                        <graphic facs="KR3fa005/8543744A_AFB9_4A55_88C8_074E306A6BC9.png" type="inline"/>
                    </figure>
                    <seg xml:id="KR3fa005_tls_p2.s10" state="locked">
                        <lb n="10" ed="KX"/>
                        <space n="䶻䶻" quantity="2"/>亢</seg>
                    <seg xml:id="KR3fa005_tls_p2.s11" state="locked">
                        <lb n="11" ed="KX"/>四星恰似彎弓狀大角一星直上明折威七子亢下</seg>
                    <seg xml:id="KR3fa005_tls_p2.s12" state="locked">
                        <lb n="12" ed="KX"/>横大角左右攝提星三<space n="䶻" quantity="1"/>相似如𪔂形折威下左頓</seg>
                    <seg xml:id="KR3fa005_tls_p2.s13" state="locked">
                        <lb n="13" ed="KX"/>頑星兩箇斜安黃色精頑下二星號陽門色若頓頑</seg>
                    <seg xml:id="KR3fa005_tls_p2.s14" state="locked">
                        <lb n="14" ed="KX"/>直下蹲</seg>
                    <pb ed="KX" n="DLJ0031362.3" xml:id="KR3fa005_tls_3a"/>
                    <figure>
                        <graphic facs="KR3fa005/C0963319_D9CA_4442_AE90_0E435CDDC1CD.png" type="inline"/>
                    </figure>
                    <seg xml:id="KR3fa005_tls_p3.s10" state="locked">
                        <lb n="10" ed="KX"/>
                        <space n="䶻䶻" quantity="2"/>氐</seg>
                    <seg xml:id="KR3fa005_tls_p3.s11" state="locked">
                        <lb n="11" ed="KX"/>四星似斗側量米天乳氐上黑一星世人不識稱無</seg>
                    <seg xml:id="KR3fa005_tls_p3.s12" state="locked">
                        <lb n="12" ed="KX"/>名一個招搖梗河上梗河横列三星狀帝席三黑河</seg>
                    <seg xml:id="KR3fa005_tls_p3.s13" state="locked">
                        <lb n="13" ed="KX"/>之西亢池六星近攝提氐下衆星騎官出騎官之衆</seg>
                    <seg xml:id="KR3fa005_tls_p3.s14" state="locked">
                        <lb n="14" ed="KX"/>二十七三三相連十欠一陣車氐下騎官次騎官下</seg>
                    <seg xml:id="KR3fa005_tls_p3.s15" state="locked">
                        <lb n="15" ed="KX"/>三車騎位天輻兩星立陳旁將軍陳裏振威霜</seg>
                    <pb ed="KX" n="DLJ0031362.4" xml:id="KR3fa005_tls_4a"/>
                    <figure>
                        <graphic facs="KR3fa005/5626F534_F82C_47B7_8318_1175D9E1AB46.png" type="inline"/>
                    </figure>
                    <seg xml:id="KR3fa005_tls_p4.s10" state="locked">
                        <lb n="10" ed="KX"/>
                        <space n="䶻䶻" quantity="2"/>房</seg>
                    <seg xml:id="KR3fa005_tls_p4.s11" state="locked">
                        <lb n="11" ed="KX"/>四星直下主明堂鍵閉一黃斜向上鉤鈐兩個近其</seg>
                    <seg xml:id="KR3fa005_tls_p4.s12" state="locked">
                        <lb n="12" ed="KX"/>傍罰有三星植鍵上兩咸夾罰似房状房下一星號</seg>
                    <seg xml:id="KR3fa005_tls_p4.s13" state="locked">
                        <lb n="13" ed="KX"/>為日從官兩個日下出</seg>
                    <pb ed="KX" n="DLJ0031362.5" xml:id="KR3fa005_tls_5a"/>
                    <figure>
                        <graphic facs="KR3fa005/508E1000_18C9_4D11_BDF0_9813750540E7.png" type="inline"/>
                    </figure>
                    <seg xml:id="KR3fa005_tls_p5.s10" state="locked">
                        <lb n="10" ed="KX"/>
                        <space n="䶻䶻" quantity="2"/>心</seg>
                    <seg xml:id="KR3fa005_tls_p5.s11" state="locked">
                        <lb n="11" ed="KX"/>三星中央色最深下有積卒共十二三三相聚心下</seg>
                    <seg xml:id="KR3fa005_tls_p5.s12" state="locked">
                        <lb n="12" ed="KX"/>是</seg>
                    <pb ed="KX" n="DLJ0031362.6" xml:id="KR3fa005_tls_6a"/>
                    <figure>
                        <graphic facs="KR3fa005/5C549720_6A30_490F_8AB8_C1B3E5A4466C.png" type="inline"/>
                    </figure>
                    <seg xml:id="KR3fa005_tls_p6.s10" state="locked">
                        <lb n="10" ed="KX"/>
                        <space n="䶻䶻" quantity="2"/>尾</seg>
                    <seg xml:id="KR3fa005_tls_p6.s11" state="locked">
                        <lb n="11" ed="KX"/>九星如鈎蒼龍尾下頭五㸃號龜星尾上天江四横</seg>
                    <seg xml:id="KR3fa005_tls_p6.s12" state="locked">
                        <lb n="12" ed="KX"/>是尾東一個名傅説傅説東畔一魚星龜西一室是</seg>
                    <seg xml:id="KR3fa005_tls_p6.s13" state="locked">
                        <lb n="13" ed="KX"/>神宫所以列在后𡚱中</seg>
                    <pb ed="KX" n="DLJ0031362.7" xml:id="KR3fa005_tls_7a"/>
                    <figure>
                        <graphic facs="KR3fa005/DAC76293_7F0E_482F_B38F_F355D0EA13F8.png" type="inline"/>
                    </figure>
                    <seg xml:id="KR3fa005_tls_p7.s10" state="locked">
                        <lb n="10" ed="KX"/>
                        <space n="䶻䶻" quantity="2"/>箕</seg>
                    <seg xml:id="KR3fa005_tls_p7.s11" state="locked">
                        <lb n="11" ed="KX"/>四星形狀如簸箕箕下三星名木杵箕前一黑是糠</seg>
                    <seg xml:id="KR3fa005_tls_p7.s12" state="locked">
                        <lb n="12" ed="KX"/>皮</seg>
                    <pb ed="KX" n="DLJ0031362.8" xml:id="KR3fa005_tls_8a"/>
                    <figure>
                        <graphic facs="KR3fa005/5D9FB1AF_DF61_4FD4_A612_319D02001C4A.png" type="inline"/>
                    </figure>
                    <seg xml:id="KR3fa005_tls_p8.s10" state="locked">
                        <lb n="10" ed="KX"/>
                        <space n="䶻䶻" quantity="2"/>北方斗</seg>
                    <seg xml:id="KR3fa005_tls_p8.s11" state="locked">
                        <lb n="11" ed="KX"/>六星其狀似北斗魁上建星三相對天弁建上三三</seg>
                    <seg xml:id="KR3fa005_tls_p8.s12" state="locked">
                        <lb n="12" ed="KX"/>九斗下圓安十四星雖然名鼈貫索形天雞建背𩀱</seg>
                    <seg xml:id="KR3fa005_tls_p8.s13" state="locked">
                        <lb n="13" ed="KX"/>黑星天籥柄前八黃精狗國四方雞下生天淵十星</seg>
                    <seg xml:id="KR3fa005_tls_p8.s14" state="locked">
                        <lb n="14" ed="KX"/>鼈東邉更有兩狗斗魁前農家丈人狗下眠天淵十</seg>
                    <seg xml:id="KR3fa005_tls_p8.s15" state="locked">
                        <lb n="15" ed="KX"/>黃狗色𤣥</seg>
                    <pb ed="KX" n="DLJ0031362.9" xml:id="KR3fa005_tls_9a"/>
                    <figure>
                        <graphic facs="KR3fa005/4A47AC06_5B15_4BE1_8F5D_300757704EEE.png" type="inline"/>
                    </figure>
                    <seg xml:id="KR3fa005_tls_p9.s10" state="locked">
                        <lb n="10" ed="KX"/>
                        <space n="䶻䶻" quantity="2"/>牛</seg>
                    <seg xml:id="KR3fa005_tls_p9.s11" state="locked">
                        <lb n="11" ed="KX"/>六星近在河岸頭頭上雖然有兩角腹下從來欠一</seg>
                    <seg xml:id="KR3fa005_tls_p9.s12" state="locked">
                        <lb n="12" ed="KX"/>脚牛下九黑是天田田下三三九坎連牛上直建三</seg>
                    <seg xml:id="KR3fa005_tls_p9.s13" state="locked">
                        <lb n="13" ed="KX"/>河鼓鼓上三星號織女左旗右旗各九星河鼓兩畔</seg>
                    <seg xml:id="KR3fa005_tls_p9.s14" state="locked">
                        <lb n="14" ed="KX"/>右邉明更有四黃名天桴河鼓直下如連珠羅堰三</seg>
                    <seg xml:id="KR3fa005_tls_p9.s15" state="locked">
                        <lb n="15" ed="KX"/>烏牛東居漸臺四星似口形輦道東足連五丁輦道</seg>
                    <seg xml:id="KR3fa005_tls_p9.s16" state="locked">
                        <lb n="16" ed="KX"/>漸臺在何許欲得見時近織女</seg>
                    <pb ed="KX" n="DLJ0031362.10" xml:id="KR3fa005_tls_10a"/>
                    <figure>
                        <graphic facs="KR3fa005/C6F49FBC_01F6_4A16_BFCD_139DA376534B.png" type="inline"/>
                    </figure>
                    <seg xml:id="KR3fa005_tls_p10.s10" state="locked">
                        <lb n="10" ed="KX"/>
                        <space n="䶻䶻" quantity="2"/>女</seg>
                    <seg xml:id="KR3fa005_tls_p10.s11" state="locked">
                        <lb n="11" ed="KX"/>四星如箕主嫁娶十二諸國在下陳先從越國向東</seg>
                    <seg xml:id="KR3fa005_tls_p10.s12" state="locked">
                        <lb n="12" ed="KX"/>論東西兩周次二秦雍州南下雙雁門代國向西一</seg>
                    <seg xml:id="KR3fa005_tls_p10.s13" state="locked">
                        <lb n="13" ed="KX"/>𣈆伸韓魏各一𣈆北輪楚之一國魏西屯楚城南畔</seg>
                    <seg xml:id="KR3fa005_tls_p10.s14" state="locked">
                        <lb n="14" ed="KX"/>獨燕軍燕西一郡是齊鄰齊北兩邑平原君欲知鄭</seg>
                    <seg xml:id="KR3fa005_tls_p10.s15" state="locked">
                        <lb n="15" ed="KX"/>在越下存十六黃星細區分五個離珠女上星敗𤓰</seg>
                    <seg xml:id="KR3fa005_tls_p10.s16" state="locked">
                        <lb n="16" ed="KX"/>珠上瓠𤓰生兩個各五瓠𤓰明天津九個弹弓形兩</seg>
                    <seg xml:id="KR3fa005_tls_p10.s17" state="locked">
                        <lb n="17" ed="KX"/>星入牛河中横四個奚仲天津上七個仲側扶箱星</seg>
                    <pb ed="KX" n="DLJ0031362.11" xml:id="KR3fa005_tls_11a"/>
                    <figure>
                        <graphic facs="KR3fa005/CB407D84_A135_4A00_9231_29644D5329B6.png" type="inline"/>
                    </figure>
                    <seg xml:id="KR3fa005_tls_p11.s10" state="locked">
                        <lb n="10" ed="KX"/>
                        <space n="䶻䶻" quantity="2"/>虚</seg>
                    <seg xml:id="KR3fa005_tls_p11.s11" state="locked">
                        <lb n="11" ed="KX"/>上下各一如連珠命祿危非虗上呈虗危之下哭泣</seg>
                    <seg xml:id="KR3fa005_tls_p11.s12" state="locked">
                        <lb n="12" ed="KX"/>星哭泣雙𩀱下壘城天壘團圓十三星敗臼四星城</seg>
                    <seg xml:id="KR3fa005_tls_p11.s13" state="locked">
                        <lb n="13" ed="KX"/>下横臼西三個離瑜明</seg>
                    <pb ed="KX" n="DLJ0031362.12" xml:id="KR3fa005_tls_12a"/>
                    <figure>
                        <graphic facs="KR3fa005/0C9E1994_74B4_4C49_97BE_07CF6839A001.png" type="inline"/>
                    </figure>
                    <seg xml:id="KR3fa005_tls_p12.s10" state="locked">
                        <lb n="10" ed="KX"/>
                        <space n="䶻䶻" quantity="2"/>危</seg>
                    <seg xml:id="KR3fa005_tls_p12.s11" state="locked">
                        <lb n="11" ed="KX"/>三星不直觀先知危上五黑號人星人下三四杵臼</seg>
                    <seg xml:id="KR3fa005_tls_p12.s12" state="locked">
                        <lb n="12" ed="KX"/>形人上七烏號車府府上天鈎九黃晶鈎上五鵶字</seg>
                    <seg xml:id="KR3fa005_tls_p12.s13" state="locked">
                        <lb n="13" ed="KX"/>造父危下四星號墳墓墓下四星斜虗梁十個天錢</seg>
                    <seg xml:id="KR3fa005_tls_p12.s14" state="locked">
                        <lb n="14" ed="KX"/>梁下黃墓旁兩星能葢屋身著黑衣危下㝛</seg>
                    <pb ed="KX" n="DLJ0031362.13" xml:id="KR3fa005_tls_13a"/>
                    <figure>
                        <graphic facs="KR3fa005/C0ECBE27_E5BF_4CBD_92CE_F160569BC68B.png" type="inline"/>
                    </figure>
                    <seg xml:id="KR3fa005_tls_p13.s10" state="locked">
                        <lb n="10" ed="KX"/>
                        <space n="䶻䶻" quantity="2"/>室</seg>
                    <seg xml:id="KR3fa005_tls_p13.s11" state="locked">
                        <lb n="11" ed="KX"/>兩星上有離宫出遶室三𩀱有六星下頭六個雷電</seg>
                    <seg xml:id="KR3fa005_tls_p13.s12" state="locked">
                        <lb n="12" ed="KX"/>形壘壁陳次十二星十二兩頭大似井陳下分布羽</seg>
                    <seg xml:id="KR3fa005_tls_p13.s13" state="locked">
                        <lb n="13" ed="KX"/>林軍四十五卒三為羣軍西四星多難論子細歴歴</seg>
                    <seg xml:id="KR3fa005_tls_p13.s14" state="locked">
                        <lb n="14" ed="KX"/>看區分三粒黃金名鈇鉞一顆真珠北落門門東八</seg>
                    <seg xml:id="KR3fa005_tls_p13.s15" state="locked">
                        <lb n="15" ed="KX"/>魁九個子門西一宿天綱是電旁兩黑土公吏螣蛇</seg>
                    <seg xml:id="KR3fa005_tls_p13.s16" state="locked">
                        <lb n="16" ed="KX"/>室上二十二</seg>
                    <pb ed="KX" n="DLJ0031362.14" xml:id="KR3fa005_tls_14a"/>
                    <figure>
                        <graphic facs="KR3fa005/17E278ED_1D5E_4ABA_BB6E_91319BF4D741.png" type="inline"/>
                    </figure>
                    <seg xml:id="KR3fa005_tls_p14.s10" state="locked">
                        <lb n="10" ed="KX"/>
                        <space n="䶻䶻" quantity="2"/>壁</seg>
                    <seg xml:id="KR3fa005_tls_p14.s11" state="locked">
                        <lb n="11" ed="KX"/>兩星下頭是霹靂霹靂五星横着行雲雨次之曰四</seg>
                    <seg xml:id="KR3fa005_tls_p14.s12" state="locked">
                        <lb n="12" ed="KX"/>方壁上天廐十圓黃鈇鑕五星羽林旁</seg>
                    <pb ed="KX" n="DLJ0031362.15" xml:id="KR3fa005_tls_15a"/>
                    <figure>
                        <graphic facs="KR3fa005/A4227531_A265_410B_8FCD_7F04160AEC42.png" type="inline"/>
                    </figure>
                    <seg xml:id="KR3fa005_tls_p15.s10" state="locked">
                        <lb n="10" ed="KX"/>
                        <space n="䶻䶻" quantity="2"/>西方奎</seg>
                    <seg xml:id="KR3fa005_tls_p15.s11" state="locked">
                        <lb n="11" ed="KX"/>腰細頭尖似破鞵一十六星遶鞵生外屏七烏奎下</seg>
                    <seg xml:id="KR3fa005_tls_p15.s12" state="locked">
                        <lb n="12" ed="KX"/>横屏下七星天溷明司空左畔土之精奎上一宿軍</seg>
                    <seg xml:id="KR3fa005_tls_p15.s13" state="locked">
                        <lb n="13" ed="KX"/>南門河中六個閣道形附路一星道旁明五個吐花</seg>
                    <seg xml:id="KR3fa005_tls_p15.s14" state="locked">
                        <lb n="14" ed="KX"/>王良星良星近上一策名</seg>
                    <pb ed="KX" n="DLJ0031362.16" xml:id="KR3fa005_tls_16a"/>
                    <figure>
                        <graphic facs="KR3fa005/269D913D_82DA_4E7B_84BD_82B8043B1E51.png" type="inline"/>
                    </figure>
