                    <seg xml:id="KR3fa018_tls_p1.s18" state="locked">
                        <lb n="18" ed="KX"/>
                        <space n="䶻" quantity="1"/>井三十三度三十分<space n="䶻" quantity="1"/>鬼二度二十分<space n="䶻" quantity="1"/>柳十三度三十分</seg>
                    <seg xml:id="KR3fa018_tls_p1.s19" state="locked">
