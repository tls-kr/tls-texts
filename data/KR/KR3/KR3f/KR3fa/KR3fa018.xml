                    <seg xml:id="KR3fa018_tls_p2.s13" state="locked">
                        <lb n="13" ed="KX"/>
                        <space n="䶻䶻䶻" quantity="3"/>周天三百六十五度四分度之一二百八十三座</seg>
                    <seg xml:id="KR3fa018_tls_p2.s14" state="locked">
                        <lb n="14" ed="KX"/>
                        <space n="䶻䶻䶻䶻" quantity="4"/>一千四百六十四星</seg>
                    <seg xml:id="KR3fa018_tls_p2.s15" state="locked">
                        <lb n="15" ed="KX"/>天漢起東方尾箕之閒謂之漢津始經龜魚傳説天</seg>
                    <seg xml:id="KR3fa018_tls_p2.s16" state="locked">
                        <lb n="16" ed="KX"/>江糠星天籥斜行上連箕斗天弁河鼓左右旗上倒</seg>
                    <seg xml:id="KR3fa018_tls_p2.s17" state="locked">
                        <lb n="17" ed="KX"/>分一派西映天市之吳越宗人宗星而止其大勢上</seg>
                    <seg xml:id="KR3fa018_tls_p2.s18" state="locked">
                        <lb n="18" ed="KX"/>絡天津車府造父螣蛇附路閣道天船大陵漸下而</seg>
                    <seg xml:id="KR3fa018_tls_p2.s19" state="locked">
                        <lb n="19" ed="KX"/>東南行歷卷右五車天潢天關司怪水府拂諸王入</seg>
                    <seg xml:id="KR3fa018_tls_p2.s20" state="locked">
                        <lb n="20" ed="KX"/>東井過四瀆闕丘天狗弧矢天社天稷在七星南而</seg>
                    <seg xml:id="KR3fa018_tls_p2.s21" state="locked">
                        <lb n="21" ed="KX"/>没</seg>
