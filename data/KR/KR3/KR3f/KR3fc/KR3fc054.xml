                    <seg state="locked" xml:id="KR3fc054_tls_p8.s13.1">
                        <lb n="13" ed="KX"/>大寒</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s13.2" type="comm">十二月中<lb type="/"/>坎六三</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s13.3">雞始乳鷙鳥厲疾水澤腹堅公升辟臨侯小過</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s13.4" type="comm">內</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s14.1">
                        <lb n="14" ed="KX"/>立春</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s14.2" type="comm">正月節<lb type="/"/>坎六四</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s14.3">東風解凍蟄蟲始振魚上冰侯小過</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s14.4" type="comm">外</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s14.5">大夫䝉卿益</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s15.1">
                        <lb n="15" ed="KX"/>雨水</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s15.2" type="comm">正月中<lb type="/"/>坎九五</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s15.3">獺祭魚鴻鴈來草木萌動公漸辟泰侯需</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s15.4" type="comm">內</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s16.1">
                        <lb n="16" ed="KX"/>驚蟄</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s16.2" type="comm">二月節<lb type="/"/>坎□</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s16.3">桃始華倉庚嗚鷹化爲鳩侯需</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s16.4" type="comm">外</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s16.5">大夫隨卿晉</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s17.1">
                        <lb n="17" ed="KX"/>春分</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s17.2" type="comm">二月中<lb type="/"/>震初九</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s17.3">元鳥至雷乃發聲始電公解辟大壯侯豫</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s17.4" type="comm">內</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s18.1">
                        <lb n="18" ed="KX"/>清明</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s18.2" type="comm">三月節<lb type="/"/>震六二</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s18.3">桐始華田鼠化駕虹始見侯豫</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s18.4" type="comm">外</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s18.5">大夫訟卿蠱</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s19.1">
                        <lb n="19" ed="KX"/>穀雨</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s19.2" type="comm">三月中<lb type="/"/>震六三</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s19.3">萍始生鳴鳩拂羽載勝降桑公革辟夬侯旅</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s19.4" type="comm">內</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s20.1">
                        <lb n="20" ed="KX"/>立夏</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s20.2" type="comm">四月節<lb type="/"/>震儿六</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s20.3">螻蟈嗚蚯蚓出王□生侯旅</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s20.4" type="comm">外</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s20.5">大夫師卿比</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s21.1">
                        <lb n="21" ed="KX"/>小滿</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s21.2" type="comm">四月中<lb type="/"/>震六五</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s21.3">苦菜秀靡早死小暑至公小畜辟乾侯大有</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p8.s21.4" type="comm">內</seg>
                    <pb ed="KX" n="03-07-101.9" xml:id="KR3fc054_tls_9a"/>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s1.1">
                        <lb n="1" ed="KX"/>芒種</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s1.2" type="comm">五月節<lb type="/"/>震十六</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s1.3">螗螂生□始鳴反舌無聲侯大有</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s1.4" type="comm">外</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s1.5">大夫家人卿井</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s2.1">
                        <lb n="2" ed="KX"/>夏至</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s2.2" type="comm">五月中<lb type="/"/>離初九</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s2.3">鹿角解蜩始嗚半夏生公咸辟姤侯鼎</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s2.4" type="comm">內</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s3.1">
                        <lb n="3" ed="KX"/>小暑</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s3.2" type="comm">六月節<lb type="/"/>離六二</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s3.3">溫風至蟋蟀居壁鷹乃學習侯鼎</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s3.4" type="comm">外</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s3.5">大夫豐卿涣</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s4.1">
                        <lb n="4" ed="KX"/>大暑</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s4.2" type="comm">六月中<lb type="/"/>離九三</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s4.3">腐草爲螢土潤溽暑大雨時行公履辟遯侯恒</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s4.4" type="comm">內</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s5.1">
                        <lb n="5" ed="KX"/>立秋</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s5.2" type="comm">七月節<lb type="/"/>離九四</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s5.3">涼風至白露降寒蟬嗚侯恒</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s5.4" type="comm">外</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s5.5">大夫節卿同人</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s6.1">
                        <lb n="6" ed="KX"/>處暑</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s6.2" type="comm">七月中<lb type="/"/>離六五</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s6.3">鷹乃祭烏天地始肅禾乃登公損辟否侯巽</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s6.4" type="comm">內</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s7.1">
                        <lb n="7" ed="KX"/>白露</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s7.2" type="comm">八月節<lb type="/"/>離上九</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s7.3">鴻鴈來元鳥歸羣鳥養羞侯巽</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s7.4" type="comm">外</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s7.5">大夫萃卿大□</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s8.1">
                        <lb n="8" ed="KX"/>秋分</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s8.2" type="comm">八月中<lb type="/"/>兌初九</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s8.3">雷乃收聲蟄蟲坏戶水始涸公賁辟觀侯歸妹</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s8.4" type="comm">內</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s9.1">
                        <lb n="9" ed="KX"/>寒露</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s9.2" type="comm">九月節<lb type="/"/>兌九二</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s9.3">鴻鴈來賓雀水爲蛤菊有黄華侯歸妹</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s9.4" type="comm">外</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s9.5">大夫旡妄卿明夷</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s10.1">
                        <lb n="10" ed="KX"/>霜降</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s10.2" type="comm">九月中<lb type="/"/>兌六三</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s10.3">脟□乃祭獸草木黃落蟄蟲咸俯公困辟剝侯艮</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s10.4" type="comm">內</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s12.1">
                        <lb n="12" ed="KX"/>立冬</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s12.2" type="comm">十月節<lb type="/"/>兌九四</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s12.3">水始冰地始凍雉入水爲蜃侯艮</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s12.4" type="comm">外</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s12.5">大夫既濟卿噬嗑</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s13.1">
                        <lb n="13" ed="KX"/>小雪</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s13.2" type="comm">十月中<lb type="/"/>兌九五</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s13.3">虹藏不見</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s13.4" type="comm">天氣上騰<lb type="/"/>地氣下降</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s13.5">閉塞成冬公大過辟坤侯未濟</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s13.6" type="comm">內</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s14.1">
                        <lb n="14" ed="KX"/>大雪</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s14.2" type="comm">十一月節<lb type="/"/>兌上六</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s14.3">□鳥不嗚虎始交荔挺出侯未濟大夫蹇卿頤</seg>
                    <seg xml:id="KR3fc054_tls_p9.s15" state="locked">
                        <lb n="15" ed="KX"/>求發斂加時各置小餘滿辰法除之爲辰數不滿者</seg>
                    <seg xml:id="KR3fc054_tls_p9.s16" state="locked">
                        <lb n="16" ed="KX"/>刻法而一爲刻又不滿爲分命辰數從子正算外即</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s17.1">
                        <lb n="17" ed="KX"/>得所求加時辰刻</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s17.2" type="comm">若以半辰之數加而命之<lb type="/"/>即得辰初後所入刻數</seg>
                    <seg xml:id="KR3fc054_tls_p9.s18" state="locked">
                        <lb n="18" ed="KX"/>求發斂去經朔置天正經朔閏餘以月閏累加之即</seg>
                    <seg xml:id="KR3fc054_tls_p9.s19" state="locked">
                        <lb n="19" ed="KX"/>每月閏餘滿元法除之爲閏日不盡爲小餘即得其</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s20.1">
                        <lb n="20" ed="KX"/>月中氣去經朔日及餘秒</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s20.2" type="comm">其閏餘滿閏限即爲置<lb type="/"/>閏以月內無中氣爲定</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s21.1">
                        <lb n="21" ed="KX"/>求卦𠊱去經朔各以卦𠊱䇿及餘秒累加減之</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p9.s21.2" type="comm">中氣<lb type="/"/>前減</seg>
                    <pb ed="KX" n="03-07-101.10" xml:id="KR3fc054_tls_10a"/>
                    <seg state="locked" xml:id="KR3fc054_tls_p10.s1.1" type="comm">
                        <lb n="1" ed="KX"/>中氣後<lb type="/"/>即加</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p10.s1.2">各得卦候去經朔日及餘秒</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p10.s2.1">
                        <lb n="2" ed="KX"/>步日躔術</seg>
                    <seg state="locked" xml:id="KR3fc054_tls_p10.s2.2" type="comm">已下<lb type="/"/>闕</seg>
                    <seg xml:id="KR3fc054_tls_p10.s3" state="locked">
                        <lb n="3" ed="KX"/>步晷漏術</seg>
                    <seg xml:id="KR3fc054_tls_p10.s4" state="locked">
                        <lb n="4" ed="KX"/>步月離術</seg>
                    <seg xml:id="KR3fc054_tls_p10.s5" state="locked">
                        <lb n="5" ed="KX"/>步交會術</seg>
                    <seg xml:id="KR3fc054_tls_p10.s6" state="locked">
                        <lb n="6" ed="KX"/>步五星術</seg>
                </p>
            </div>
