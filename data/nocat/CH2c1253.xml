<head xml:id="CH2c1253_CHANT_001-1a.1-h-h">
                    <seg xml:id="CH2c1253_CHANT_001-1a.1-h">晉錄</seg>
                </head>
            <p>
                    <seg xml:id="CH2c1253_CHANT_001-1a.1"/>
                    <seg xml:id="CH2c1253_CHANT_001-1a.2">魯芝</seg>
                </p>
            <p>
                    <seg xml:id="CH2c1253_CHANT_001-1a.3">光祿大夫魯芝<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-1a.4">清約儉嗇<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-1a.5">上賜絹三百匹<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-1a.6">
                        <c n="《"/>書鈔<c n="》"/>五十六<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1253_CHANT_001-1a.7">鎮西將軍魯芝<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-1a.8">恪勤在公<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-1a.9">不營私門<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-1a.10">素無華宅<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-1a.11">使軍兵作屋五十間<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-1a.12">
                        <c n="《"/>書鈔<c n="》"/>六十四<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1253_CHANT_001-2a.1"/>
                    <seg xml:id="CH2c1253_CHANT_001-2a.2">袁奧</seg>
                </p>
            <p>
                    <seg xml:id="CH2c1253_CHANT_001-2a.3">大中大夫新蔡男袁奧<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-2a.4">行誼優異<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-2a.5">可從九卿崇重之例<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-2a.6">
                        <c n="《"/>書鈔<c n="》"/>五十六<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1253_CHANT_001-2a.7">楊泉</seg>
                </p>
            <p>
                    <seg xml:id="CH2c1253_CHANT_001-2a.8">會稽相朱則上書言楊泉清操自然<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-2a.9">徵聘終不移心<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-2a.10">詔拜泉郎中<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-2a.11">
                        <c n="《"/>書鈔<c n="》"/>六十三<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1253_CHANT_001-3a.1"/>
                    <seg xml:id="CH2c1253_CHANT_001-3a.2">嘉瓜</seg>
                </p>
            <p>
                    <seg xml:id="CH2c1253_CHANT_001-3a.3">咸寧中<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-3a.4">嘉瓜同蔕生於成都<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-3a.5">
                        <c n="《"/>藝文<c n="》"/>八十七<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1253_CHANT_001-3a.6">白狐</seg>
                </p>
            <p>
                    <seg xml:id="CH2c1253_CHANT_001-3a.7">咸寧二年<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-3a.8">有白狐七尾見汝南<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1253_CHANT_001-3a.9">白帖狐<c n="。"/>
                    </seg>
                </p>
         </div>
