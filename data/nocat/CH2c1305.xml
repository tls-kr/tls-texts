      <body>
         <div>
                
<head xml:id="CH2c1305_CHANT_001-1a.1-h-h">
                    <seg xml:id="CH2c1305_CHANT_001-1a.1-h">田融趙書</seg>
                </head>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-1a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-1a.2">石勒與汲桑往來<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.3">汲桑清河具邱人<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.4">年二十餘<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.5">力扛百鈞<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.6">呼聞數里<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.7">時人服之<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.8">
                        <c n="《"/>御覽<c n="》"/>三百八十六<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-1a.9">汲桑自號大將軍<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.10">以勒為前驅<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.11">攻鄴<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.12">入之<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.13">南擊充州<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.14">為苟晞所敗<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.15">桑走死<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.16">汲桑六月盛暑<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.17">而垂重裘累茵<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.18">使十餘人扇之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.19">扇者悉罷<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.20">桑恚不得清涼<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.21">斬扇者<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.22">時軍中為之謠曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.23">奴為將軍何可羞<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.24">六月重茵被狐裘<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.25">不識寒暑斷人頭<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-1a.26">
                        <c n="《"/>類聚<c n="》"/>十九<c n="、"/>
                        <c n="《"/>書鈔<c n="》"/>百二十九<c n="、"/>
                        <c n="《"/>御覽<c n="》"/>二十一<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-2a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-2a.2">石勒與劉零等攻魏郡<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.3">劉零<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.4">陽平人<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.5">年二十餘<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.6">常廝役於縣<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.7">力制奔牛<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.8">走及馳馬<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.9">
                        <c n="《"/>御覽<c n="》"/>三百八十六<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-2a.10">石勒屯葛陂<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.11">值天雨不息<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.12">勒長史刁應勸勒降晉<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.13">勒愀然而嘯<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.14">張賓勸勒還北<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.15">勒欣然攘臂曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.16">
                        <c n="「"/>賓計是也<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.17">應宜斬<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.18">明其性怯<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.19">可退為將軍<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-2a.20">
                        <c n="《"/>御覽<c n="》"/>三百九十二<c n="、"/>又四百九十九<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-3a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-3a.2">石勒十四年<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-3a.3">五月<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-3a.4">飛蝗穿地而生<c n="；"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-3a.5">二十日<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-3a.6">化蠶<c n="；"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-3a.7">七八月<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-3a.8">引作蚖<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-3a.9">蚖飛<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-3a.10">周遍河朔<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-3a.11">百草無遺<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-3a.12">唯不食三豆及麻<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-3a.13">
                        <c n="《"/>開元占經<c n="》"/>百一十<c n="、"/>
                        <c n="《"/>類聚<c n="》"/>一百<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-4a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-4a.2">後石率精騎五千襲邵續<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.3">一戰<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.4">生擒續於青邱<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.5">鉗頸拲手於襄國青陽城門<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.6">頓首稱囚<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.7">
                        <c n="《"/>御覽<c n="》"/>六百四十四<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-4a.8">前石召構殿巧手三千人<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.9">發陽平等十郡<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.10">車牛五千乘<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.11">運土築建德殿臺<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.12">取土城西南墎內<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.13">得圓石<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.14">狀若水碓<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.15">其銘曰<c n="「"/>律權石<c n="」"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.16">重四鈞<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.17">上有文曰<c n="「"/>同律度量衡<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.18">有辛氏所造<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.19">議者未之詳<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.20">或以為瑞<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.21">參軍續咸曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.22">
                        <c n="「"/>王莽時物<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.23">
                        <c n="《"/>書鈔<c n="》"/>百十二<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-4a.24">
                        <c n="《"/>御覽<c n="》"/>十六<c n="、"/>又五十一<c n="、"/>一百七十五<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-5a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-5a.2">黎陽民陳武妻<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-5a.3">產三男一女<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-5a.4">勒賜乳母穀帛<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-5a.5">以為休詳<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-5a.6">
                        <c n="《"/>御覽二百一十一<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-5a.7">國豐市五日一市<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-5a.8">
                        <c n="《"/>御覽<c n="》"/>八百二十七<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-6a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-6a.2">前石數出遊獵<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.3">每亟馳騁<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.4">主簿程琅諫<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.5">
                        <c n="「"/>前石屎逐自若<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.6">草間有瓦木<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.7">馳馬觸之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.8">馬即死<c n="；"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.9">前石亦危殆<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.10">體小不穩<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.11">還宮歎曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.12">
                        <c n="「"/>程琅忠臣也<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.13">不用其言<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.14">吾之不善<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.15">追之何及<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.16">
                        <c n="《"/>御覽<c n="》"/>四百五十四<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.17">按<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.18">惟此明標<c n="《"/>田融趙書<c n="》"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.19">餘祇稱<c n="《"/>趙書<c n="》"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-6a.20">未標名<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-7a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-7a.2">劉曜攻石勒<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-7a.3">將戰曜飲<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-7a.4">乘醉<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-7a.5">乘大赤馬<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-7a.6">無故蹢躅不可近<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-7a.7">於是退赤馬<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-7a.8">及合陣<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-7a.9">敗走<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-7a.10">曜體素壯<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-7a.11">馬小不勝<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-7a.12">陷冰<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-7a.13">為石堪所擒也<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-7a.14">
                        <c n="《"/>御覽<c n="》"/>六十八<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-8a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-8a.2">佛圖澄亦謂必擒劉曜<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-8a.3">前石有佛圖澄<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-8a.4">號曰<c n="「"/>大和尚<c n="」"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-8a.5">常乘板輦於大水上<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-8a.6">
                        <c n="《"/>書鈔<c n="》"/>百四十<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-8a.7">太和二年正月<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-8a.8">具禮樂朝群后<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-8a.9">侍郎邵恭執麾不降<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-8a.10">常侍畦洪執義不彈<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-8a.11">皆下理官<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-8a.12">
                        <c n="《"/>書鈔<c n="》"/>百二十<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-9a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-9a.2">石虎等破劉曜子熙於上邽<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.3">獲馬二百疋<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.4">赤罽金銀步叉弓鞬三十具<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.5">
                        <c n="《"/>書鈔<c n="》"/>百二十六<c n="、"/>
                        <c n="《"/>御覽<c n="》"/>三百五十<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-9a.6">裴憲撰三正東耕儀<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.7">
                        <c n="《"/>書鈔<c n="》"/>百二十七<c n="、"/>又百二十九<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-9a.8">東耕儀<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.9">直殿中監鋪席於侍臣之南<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.10">北面<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.11">解匣出御耒<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.12">跪授黃門侍中<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.13">侍中釋劍擎跽以穎授尊<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.14">太常讚曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.15">
                        <c n="「"/>皇帝親耕藉田<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.16">一推一反<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.17">三推三反<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.18">成禮<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.19">侍中跽取耒與侍郎<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.20">以授殿中監<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.21">監復韜匣<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-9a.22">
                        <c n="《"/>御覽<c n="》"/>八百二十二<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-10a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-10a.2">中書令徐光秦請以東耕儀親耕<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-10a.3">改服宜服介幘<c n="、"/>青縑袴褶<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-10a.4">
                        <c n="《"/>書鈔<c n="》"/>百二十七<c n="、"/>又百三十九<c n="《"/>御覽<c n="》"/>六百九十五<c n="、"/>又八百十八<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-10a.5">徐光字秀武<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-10a.6">頓邱人<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-10a.7">父以牛醫為業<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-10a.8">光年十四五<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-10a.9">為將軍王陽秣馬<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-10a.10">光但書馬柳屋柱為詩頌<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-10a.11">不親馬事<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-10a.12">
                        <c n="《"/>類聚<c n="》"/>五十六<c n="、"/>
                        <c n="《"/>御覽<c n="》"/>□□<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-11a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-11a.2">皇后出蠶<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-11a.3">乘羽蓋金華車<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-11a.4">
                        <c n="《"/>書鈔<c n="》"/>百四十<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-11a.5">前石時<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-11a.6">暴風雷雨霹靂<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-11a.7">震臺壤署<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-11a.8">婦人震死<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-11a.9">瘞之三日<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-11a.10">重霹靂出之<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-11a.11">
                        <c n="《"/>御覽<c n="》"/>八百七十六<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-12a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-12a.2">石勒時<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-12a.3">斷寒食<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-12a.4">忽暴風霹雷雨雹<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-12a.5">大者如彈丸<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-12a.6">平地水深三尺<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-12a.7">勒大驚<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-12a.8">復令民寒食如故<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-12a.9">
                        <c n="《"/>書鈔<c n="》"/>百四十三<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-12a.10">前石臨安縣馬生角在耳<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-12a.11">長有七寸<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-12a.12">搖之則動<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-12a.13">
                        <c n="《"/>占經<c n="》"/>百十八<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-13a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-13a.2">前石時<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-13a.3">安定太守邵琨上言<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-13a.4">蛇與鼠鬭於門中<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-13a.5">而蛇自死<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-13a.6">同上一百二十<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-13a.7">前石時<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-13a.8">淳觚白羌婦人<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-13a.9">產卵大如盂<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-13a.10">剖之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-13a.11">有蟲如巨蟻<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-13a.12">二足立行<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-13a.13">
                        <c n="《"/>御覽<c n="》"/>九百四十四<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-14a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-14a.2">前石時<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.3">有石隕魏郡鄴縣東北六十里間罷地<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.4">初有黑黃雲如幕<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.5">長數十疋<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.6">交酉<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.7">聲如雷<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.8">霹靂墮地<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.9">熱氣起連天<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.10">時有左右有私鋤者<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.11">皆震疊塵止<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.12">經視之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.13">上<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.14">猶帶熱<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.15">求覓<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.16">見有一石<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.17">方一尺<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.18">青色<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.19">而輕撓之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.20">音若落<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.21">二十一日<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.22">前石死<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-14a.23">
                        <c n="《"/>占經<c n="》"/>七十六<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-15a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-15a.2">前石死<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-15a.3">調大臣子弟六十人為挽郎<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-15a.4">引錦一疋<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-15a.5">
                        <c n="《"/>御覽<c n="》"/>八百十五<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-15a.6">王洛生<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-15a.7">石勒欲挫其權豪<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-15a.8">洛生在獄自刺腹<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-15a.9">深五寸<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-15a.10">洛生肥盛<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-15a.11">不陷中<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-15a.12">重以刀潰其腹<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-15a.13">出胃而死<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-15a.14">
                        <c n="《"/>御覽<c n="》"/>百七十八<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-16a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-16a.2">石肇<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.3">前石之昆弟也<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.4">前石既貴<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.5">肇在軍中<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.6">不能自達<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.7">人送詣前石<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.8">前石哀之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.9">拜建威將軍<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.10">以肇㒇才尸<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.11">每高選參佐輔之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.12">為聘廣川劉典女<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.13">肇甚懼之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.14">拜長樂太守<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.15">治官<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.16">每入門<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.17">動稱阿劉可爾不可爾<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.18">時人以為嗤謠<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-16a.19">
                        <c n="《"/>御覽<c n="》"/>四百九十九<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-17a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-17a.2">石勒參軍周延<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.3">為館陶令<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.4">盜官絹數百疋<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.5">下獄<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.6">以八座議宥之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.7">後每設大會<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.8">使與俳兒著介幘黃絹單衣<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.9">優問延曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.10">
                        <c n="「"/>汝為何官<c n="？"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.11">在我俳中<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.12">答曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.13">
                        <c n="「"/>我本為館陶令<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.14">計二十數<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.15">單衣曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.16">
                        <c n="「"/>政坐取<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.17">是故入汝俳中<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.18">以為大笑<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-17a.19">
                        <c n="《"/>類聚<c n="》"/>八十五<c n="、"/>
                        <c n="《"/>書鈔<c n="》"/>百十二<c n="、"/>
                        <c n="《"/>御覽<c n="》"/>五百六十九<c n="、"/>又六百九十一<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-18a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-18a.2">石勒使王述煮鹽於角飛<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-18a.3">
                        <c n="《"/>水經注<c n="》"/>九<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-18a.4">按<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-18a.5">此引作<c n="《"/>趙記<c n="》"/>
                        <c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-18a.6">石虎字季龍<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-18a.7">勒從弟也<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-18a.8">征伐每斬將搴旗<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-18a.9">勒死<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-18a.10">誅勒諸兒<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-18a.11">襲位<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-18a.12">
                        <c n="《"/>世說<c n="》"/>注二<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-19a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-19a.2">將軍張珍<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-19a.3">飲郡縣民丁萬人<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-19a.4">徙洛陽六鐘<c n="、"/>鐘簴<c n="、"/>九龍<c n="、"/>翁仲<c n="、"/>銅駞<c n="、"/>飛廉鐘一<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-19a.5">沒盟津中<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-19a.6">
                        <c n="《"/>御覽<c n="》"/>五百七十五<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-19a.7">石虎造庭燎於橦末<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-19a.8">高十丈<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-19a.9">上盤置燎<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-19a.10">下盤安人以待燎<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-19a.11">絙繳上下<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-19a.12">
                        <c n="《"/>類聚<c n="》"/>八十<c n="、"/>
                        <c n="《"/>御覽<c n="》"/>八百七十<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-20a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-20a.2">石虎聘崔氏為夫人<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-20a.3">無寵<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-20a.4">所愛鄭夫人<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-20a.5">有百日女<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-20a.6">病<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-20a.7">謂崔誤與藥<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-20a.8">以告後石<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-20a.9">虎作威問之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-20a.10">崔言外合見小子<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-20a.11">以少唾其容作祟<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-20a.12">非藥也<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-20a.13">後石乃射之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-20a.14">一箭通中而死<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-20a.15">
                        <c n="《"/>御覽<c n="》"/>三百八十七<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-21a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-21a.2">從幽州大道滹沱河造浮橋<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.3">植行榆五十里<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.4">置行宮<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.5">
                        <c n="《"/>類聚<c n="》"/>八十八<c n="、"/>
                        <c n="《"/>御覽<c n="》"/>九百五十六<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-21a.6">臨元侯崔約<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.7">字道恭<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.8">與太子詹事羯孫珍朝會<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.9">珍患目痛<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.10">問約用何方治為佳<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.11">約戲言唯溺取愈<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.12">珍曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.13">
                        <c n="「"/>目何可溺<c n="？"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.14">約曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.15">
                        <c n="「"/>卿目睕睕<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.16">正可溺中<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.17">珍入秦<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.18">天子怒<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.19">約父子伏誅<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-21a.20">
                        <c n="《"/>御覽<c n="》"/>六百四十五<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-22a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-22a.2">後石造獵車千<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-22a.3">轅長三尺<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-22a.4">
                        <c n="《"/>御覽<c n="》"/>七百七十五<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-22a.5">石虎子韜<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-22a.6">以琉璃爵<c n="、"/>螺杯<c n="、"/>勸客酒<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-22a.7">
                        <c n="《"/>御覽<c n="》"/>七百六十<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-22a.8">石虎建武十四年<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-22a.9">東南卒有雲黑<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-22a.10">筲分為三疋<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-22a.11">又貫日日沒後分為七枚<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-22a.12">相去數十丈<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-22a.13">其間有白雲如魚鱗<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-22a.14">虎子韜曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-22a.15">
                        <c n="「"/>當有刺客<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-22a.16">後果為太子宣所殺<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-22a.17">
                        <c n="《"/>御覽<c n="》"/>八百七十七<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-23a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-23a.2">前石有佛圖澄<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.3">號曰<c n="「"/>大和尚<c n="」"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.4">道法大行<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.5">建武末卒<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.6">葬於鄴西紫陌<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.7">先造生墓<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.8">已數年矣<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.9">
                        <c n="《"/>書鈔<c n="》"/>九十二<c n="、"/>
                        <c n="《"/>御覽<c n="》"/>百九十五<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-23a.10">宣咸卒後五年<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.11">石虎夢見咸<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.12">涕泗囑其子奮<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.13">虎曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.14">
                        <c n="「"/>非心慮所達也<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.15">通夢之言<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.16">信而有徵<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.17">奮今何在<c n="？"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.18">左右對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.19">
                        <c n="「"/>為趙郡守<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.20">於是握廷尉為太常<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.21">才力不及父<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.22">因咸夢而登列卿也<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-23a.23">
                        <c n="《"/>太平廣記<c n="》"/>二百七十六<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-24a.1"/>
                    <seg xml:id="CH2c1305_CHANT_001-24a.2">冉閔攻石遵<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.3">遵方與女子彈碁<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.4">兵至<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.5">殺之<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.6">
                        <c n="《"/>御覽<c n="》"/>七百五十四<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2c1305_CHANT_001-24a.7">燕人龐世為光祿勳<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.8">秦案豪強苛剋<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.9">人物咸懼疾之<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.10">及卒<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.11">門無弔客<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.12">時人為之謠曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.13">
                        <c n="「"/>龐家之巷<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.14">車馬鱗鱗<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.15">泥丸之日<c n="，"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.16">門無弔賓<c n="。"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.17">弔賓不來何所因<c n="？"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.18">由性苛剋寡所親<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2c1305_CHANT_001-24a.19">
                        <c n="《"/>御覽<c n="》"/>百四六十五<c n="。"/>
                    </seg>
                </p>
         </div>
      </body>
   </text>
</TEI>