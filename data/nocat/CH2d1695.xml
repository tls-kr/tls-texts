                    <seg xml:id="CH2d1695_CHANT_001-1a.1"/>
                    <seg xml:id="CH2d1695_CHANT_001-1a.2">金城郡<c n="，"/>
                    </seg>
                    <seg xml:id="CH2d1695_CHANT_001-1a.3">漢元始六年置<c n="。"/>
                    </seg>
                    <seg xml:id="CH2d1695_CHANT_001-1a.4">應劭曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2d1695_CHANT_001-1a.5">初築城得金<c n="，"/>
                    </seg>
                    <seg xml:id="CH2d1695_CHANT_001-1a.6">故曰金城<c n="。"/>
                    </seg>
                </p>
