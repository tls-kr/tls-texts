      <body>
         <div>
                
<head xml:id="CH2e1112_CHANT_001-1a.1-h-h">
                    <seg xml:id="CH2e1112_CHANT_001-1a.1-h">皇覽逸禮</seg>
                </head>
            <p>
                    <seg xml:id="CH2e1112_CHANT_001-1a.1"/>
                    <seg xml:id="CH2e1112_CHANT_001-1a.2">天子迎四節日<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.3">天子迎春<c n="、"/>夏<c n="、"/>秋<c n="、"/>冬之樂<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.4">又順天道<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.5">是故距冬至四十六日<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.6">則天子迎春於冬堂<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.7">距邦八里<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.8">堂高八尺<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.9">堂階八等<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.10">青稅八乘<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.11">旗旄尚青<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.12">田車載矛<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.13">號曰助天生<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.14">唱之以角<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.15">舞之以羽翟<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.16">此迎春之樂也<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.17">自春分數四十六日<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.18">則天子迎夏於南堂<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.19">距邦七里<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.20">堂高七尺<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.21">堂階七等<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.22">赤稅七乘<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.23">旗旄尚赤<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.24">田車載弓<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.25">號曰助天養<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.26">唱之以徵<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.27">舞之以鼓鞉<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.28">此迎夏之樂也<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.29">自夏至數四十六日<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.30">則天子迎秋於西堂<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.31">距邦九里<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.32">堂高九尺<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.33">堂階九等<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.34">白稅九乘<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.35">旗旄尚白<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.36">田車載戟<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.37">號助天收<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.38">唱之以商<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.39">舞之以干戚<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.40">此迎秋之樂也<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.41">自秋分數四十六日<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.42">則天子迎冬於北堂<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.43">距邦六里<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.44">堂高六尺<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.45">堂階六等<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.46">黑稅六乘<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.47">旗旄尚黑<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.48">田車載甲鐵鍪<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.49">號曰助天誅<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.50">唱之以羽<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.51">舞之以籥<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.52">此迎冬之樂也<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.53">所以迎四時樂<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.54">秋養九志於西堂<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.55">冬養九勝於北堂<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.56">養後三日而<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.57">止天子行殺<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.58">必順天道<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-1a.59">
                        <c n="《"/>御覽<c n="》"/>
                        <c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2e1112_CHANT_001-2a.1"/>
                    <seg xml:id="CH2e1112_CHANT_001-2a.2">春則衣青衣<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.3">佩蒼玉<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.4">乘青輅<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.5">駕青騮<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.6">載青旗<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.7">以迎春於東郊<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.8">居明堂<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.9">左啟東戶<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2e1112_CHANT_001-2a.10">夏則衣赤衣<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.11">佩赤玉<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.12">乘赤輅<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.13">駕赤騮<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.14">載赤旗<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.15">以迎夏於南郊<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.16">食菽<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.17">其祭<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.18">先黍與雞明堂正居廟<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.19">啟南戶<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-2a.20">
                        <c n="《"/>書鈔<c n="》"/>引此云<c n="「"/>仲夏則居明堂正廟<c n="」"/>
                        <c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2e1112_CHANT_001-3a.1"/>
                    <seg xml:id="CH2e1112_CHANT_001-3a.2">秋則衣白衣<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.3">佩白玉<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.4">乘白輅<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.5">駕白騮<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.6">載白旗<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.7">以迎秋於西郊<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.8">居明堂<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.9">右啟西戶<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2e1112_CHANT_001-3a.10">冬則衣黑衣<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.11">佩玄玉<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.12">乘玄輅<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.13">駕鐵驪<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.14">載玄旂<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.15">以迎冬於北郊<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.16">其祭<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.17">先黍與豕<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.18">居明堂後廟<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-3a.19">啟北戶<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2e1112_CHANT_001-4a.1"/>
                    <seg xml:id="CH2e1112_CHANT_001-4a.2">天子之蓍九尺<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.3">諸侯七尺<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.4">丈夫五尺<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.5">士三尺<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.6">蓍千歲三百莖者<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.7">先知也<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.8">並<c n="《"/>類聚<c n="》"/>
                        <c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2e1112_CHANT_001-4a.9">天子龜尺二寸<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.10">諸侯八寸<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.11">大夫六寸<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.12">士民四寸<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.13">龜者<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.14">陰蟲之老也<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.15">龜三千歲上<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.16">游於卷耳之上<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.17">老者先知<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.18">故君子舉事必考之<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-4a.19">
                        <c n="《"/>初學記<c n="》"/>
                        <c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2e1112_CHANT_001-5a.1"/>
                    <seg xml:id="CH2e1112_CHANT_001-5a.2">君使大夫弔於國君<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-5a.3">禮錫衰衰裳<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-5a.4">弁經有經下馬<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-5a.5">大夫為介<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-5a.6">亦如之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-5a.7">士介將命者<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-5a.8">緦麻裳弁經<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-5a.9">異姓葛<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-5a.10">同姓麻<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2e1112_CHANT_001-6a.1"/>
                    <seg xml:id="CH2e1112_CHANT_001-6a.2">衛靈公之時<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.3">蘧伯玉賢而不用<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.4">彌子瑕不肖而任事<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.5">史鰌數言之未入<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.6">史鰌病且死<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.7">謂其子曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.8">
                        <c n="「"/>我死<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.9">則置喪於北堂<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.10">吾生不能進蘧伯玉而退彌子瑕<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.11">是不能正君也<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.12">生不能正君者<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.13">死不當成禮<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.14">死而置尸於北堂於我足矣<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.15">靈公往弔<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.16">問其故<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.17">其子以父言對於靈公<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.18">靈公失容曰<c n="："/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.19">
                        <c n="「"/>吾失矣<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.20">立召蘧伯玉而進之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.21">召彌子瑕而退之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.22">徙喪於堂<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.23">成禮而後去<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.24">衛國以治史鰌之力也<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.25">史鰌以尸諫<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.26">可謂忠不衰矣<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-6a.27">並<c n="《"/>御覽<c n="》"/>
                        <c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2e1112_CHANT_001-7a.1"/>
                    <seg xml:id="CH2e1112_CHANT_001-7a.2">凡祭五祀於廟<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.3">用特牲<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.4">有主有尸<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.5">皆先設席於奧<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.6">祀戶之禮<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.7">南面設主於戶內之西<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.8">乃制脾及腎為俎<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.9">奠於主北<c n="；"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.10">又設盛於俎西<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.11">祭黍稷<c n="、"/>祭肉<c n="、"/>祭醴<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.12">皆三<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.13">祭肉<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.14">脾一<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.15">腎再<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.16">既祭<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.17">徹之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.18">更陳鼎俎<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.19">設饌於筵前<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.20">迎尸<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-7a.21">略於祭宗廟之儀<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2e1112_CHANT_001-8a.1"/>
                    <seg xml:id="CH2e1112_CHANT_001-8a.2">祀竈之禮<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-8a.3">先席於門之奧<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-8a.4">東面<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-8a.5">設主於竈陘<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-8a.6">乃制肺及心肝為俎<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-8a.7">奠於主西<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-8a.8">又設盛於俎南<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-8a.9">亦祭黍三<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-8a.10">祭肺<c n="、"/>心<c n="、"/>肝各一<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-8a.11">祭醴一<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-8a.12">亦既祭徹之<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-8a.13">更陳鼎俎設饌於筵前<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-8a.14">迎尸如祀戶之禮<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2e1112_CHANT_001-9a.1"/>
                    <seg xml:id="CH2e1112_CHANT_001-9a.2">祀中霤之禮<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-9a.3">設主於牖下<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-9a.4">乃制心及肺肝為俎<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-9a.5">其祭肉<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-9a.6">心肺肝各一<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-9a.7">他皆如祀戶之禮<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2e1112_CHANT_001-9a.8">祀門之禮<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-9a.9">北面設主於門左樞<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-9a.10">及制肝及肺心為俎<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-9a.11">奠於主南<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-9a.12">又設盛於俎東<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-9a.13">其他皆如祭竈之禮<c n="。"/>
                    </seg>
                </p>
            <p>
                    <seg xml:id="CH2e1112_CHANT_001-10a.1"/>
                    <seg xml:id="CH2e1112_CHANT_001-10a.2">行在廟門外之西為軷壤<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-10a.3">厚二寸<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-10a.4">廣五尺<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-10a.5">輪五尺<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-10a.6">祀行之禮<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-10a.7">北面設主於軷上<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-10a.8">乃制腎及脾為俎<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-10a.9">奠於主南<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-10a.10">又設盛於東<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-10a.11">祭肉<c n="、"/>腎一<c n="、"/>脾再<c n="，"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-10a.12">其他皆如祀門之禮<c n="。"/>
                    </seg>
                    <seg xml:id="CH2e1112_CHANT_001-10a.13">並<c n="《"/>禮記<c n="‧"/>月令<c n="》"/>注<c n="。"/>
                    </seg>
                </p>
         </div>
      </body>
   </text>
