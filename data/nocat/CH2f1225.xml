            <p>
                    <seg xml:id="CH2f1225_CHANT_001-1a.1"/>
                    <seg xml:id="CH2f1225_CHANT_001-1a.2">憷<c n="，"/>
                    </seg>
                    <seg xml:id="CH2f1225_CHANT_001-1a.3">痛也<c n="。"/>
