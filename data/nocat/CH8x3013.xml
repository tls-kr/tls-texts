<head xml:id="CH8x3013_CHANT_001-1a.1-h-h">
                    <seg xml:id="CH8x3013_CHANT_001-1a.1-h">馬王堆漢墓帛書-陰陽脈死候釋文</seg>
                </head>
            <p>
                    <seg xml:id="CH8x3013_CHANT_001-1a.1"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.2"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.3">凡三陽<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.4"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.5">天氣殹<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.6"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.7">其病唯折骨列膚一死<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.8"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.9">凡三陰<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.10"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.11">地氣殹<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.12"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.13">死眽殹<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.14"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.15">陰病而亂<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.16"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.17">則不</seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.18"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.19"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.20">過十日而死<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.21"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.22" n="CH0F2263">三陰<c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>臧煉腸而主殺<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.23"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.24">□□五死<c n="；"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.25"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.26">唇反人盈<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.27"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.28">則肉先死</seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.29">
                        <c n="；"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.30"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.31">齦瘠齒長</seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.32">
                        <c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.33"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.34">則</seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.35"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.36"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.37">骨先死<c n="；"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.38"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.39">面黑<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.40"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.41" n="CH0F2264">目環視<c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>
                        <c n="（"/>衺<c n="）"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.42"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.43">則氣先死<c n="；"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.44"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.45">汗出如絲<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.46"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.47">傅而不流<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.48"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.49" n="JB001">則血先<c n="＃"/>
                        <c n="＃"/>死<c n="；"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.50"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.51" n="CH0F2265">舌掐<c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>卷<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.52"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.53">則筋</seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.54"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.55"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.56">先死<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.57"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.58">五者扁有<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.59"/>
                    <seg xml:id="CH8x3013_CHANT_001-1a.60">則不沽矣<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3013_CHANT_001-1a.61"/>
                </p>
         </div>
