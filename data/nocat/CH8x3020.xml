            <p>
                    <seg xml:id="CH8x3020_CHANT_001-13a.1"/>
                    <seg xml:id="CH8x3020_CHANT_001-13a.2"/>
                    <seg xml:id="CH8x3020_CHANT_001-13a.3">●字者<c n="，"/>
