                    <seg xml:id="CH8x3020_CHANT_001-7a.7">取蒿<c n="、"/>牡<c n="、"/>卑稍三<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3020_CHANT_001-7a.8"/>
                    <seg xml:id="CH8x3020_CHANT_001-7a.9">冶<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3020_CHANT_001-7a.10"/>
                    <seg xml:id="CH8x3020_CHANT_001-7a.11">飲之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3020_CHANT_001-7a.12"/>
                    <seg xml:id="CH8x3020_CHANT_001-7a.13">必產男<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3020_CHANT_001-7a.14"/>
                    <seg xml:id="CH8x3020_CHANT_001-7a.15">已試<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3020_CHANT_001-7a.16"/>
                    <seg xml:id="CH8x3020_CHANT_001-7a.17">一□曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3020_CHANT_001-7a.18"/>
                    <seg xml:id="CH8x3020_CHANT_001-7a.19">遺弱半升<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3020_CHANT_001-7a.20"/>
                    <seg xml:id="CH8x3020_CHANT_001-7a.21">□□堅而少汁<c n="。"/>
