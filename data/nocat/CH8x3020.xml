                    <seg xml:id="CH8x3020_CHANT_001-6a.11"/>
                    <seg xml:id="CH8x3020_CHANT_001-6a.12">取爵罋中虫青北者三<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3020_CHANT_001-6a.13"/>
                    <seg xml:id="CH8x3020_CHANT_001-6a.14">產呻之<c n="，"/>
