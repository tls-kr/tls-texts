                    <seg xml:id="CH8x3020_CHANT_001-5a.4"/>
                    <seg xml:id="CH8x3020_CHANT_001-5a.5">為享白牡狗首<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3020_CHANT_001-5a.6"/>
                    <seg xml:id="CH8x3020_CHANT_001-5a.7">令獨食之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3020_CHANT_001-5a.8"/>
                    <seg xml:id="CH8x3020_CHANT_001-5a.9">其子美皙<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3020_CHANT_001-5a.10"/>
                    <seg xml:id="CH8x3020_CHANT_001-5a.11">有易出<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3020_CHANT_001-5a.12"/>
                    <seg xml:id="CH8x3020_CHANT_001-5a.13">欲令子勁者<c n="，"/>
                    </seg>
