                    <seg xml:id="CH8x3020_CHANT_001-13a.22"/>
                    <seg xml:id="CH8x3020_CHANT_001-13a.23">其身盡得土<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3020_CHANT_001-13a.24"/>
                    <seg xml:id="CH8x3020_CHANT_001-13a.25">乃浴之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3020_CHANT_001-13a.26"/>
                    <seg xml:id="CH8x3020_CHANT_001-13a.27">為勁有力<c n="。"/>
                    </seg>
