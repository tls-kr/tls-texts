                    <seg xml:id="CH8x3020_CHANT_001-7a.3">●一曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3020_CHANT_001-7a.4"/>
                    <seg xml:id="CH8x3020_CHANT_001-7a.5">以方苴時<c n="，"/>
