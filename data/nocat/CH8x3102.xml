<head xml:id="CH8x3102_CHANT_001-1a.1-h-h">
                    <seg xml:id="CH8x3102_CHANT_001-1a.1-h">武威漢代醫簡-第二類簡</seg>
                </head>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-1a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-1a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-1a.3" resp="#chris" modified="2023-01-28T15:44:58.529+09:00">治魯氏青行解解腹方：</seg>
                    <seg xml:id="CH8x3102_CHANT_001-1a.4"/>
                    <seg xml:id="CH8x3102_CHANT_001-1a.5">麻黃卅分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-1a.6"/>
                    <seg xml:id="CH8x3102_CHANT_001-1a.7">大黃十五分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-1a.8"/>
                    <seg xml:id="CH8x3102_CHANT_001-1a.9">厚朴<c n="、"/>石膏<c n="、"/>苦參各六分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-1a.10"/>
                    <seg xml:id="CH8x3102_CHANT_001-1a.11">烏喙<c n="、"/>付子各二分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-1a.12"/>
                    <seg xml:id="CH8x3102_CHANT_001-1a.13">凡七物</seg>
                    <seg xml:id="CH8x3102_CHANT_001-1a.14"/>
                    <seg xml:id="CH8x3102_CHANT_001-1a.15">皆并冶合<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-1a.16"/>
                    <seg xml:id="CH8x3102_CHANT_001-1a.17">和以方寸匕一飲之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-1a.18"/>
                    <seg xml:id="CH8x3102_CHANT_001-1a.19">良甚<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-1a.20"/>
                    <seg xml:id="CH8x3102_CHANT_001-1a.21">皆𢠚<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-1a.22"/>
                    <seg xml:id="CH8x3102_CHANT_001-1a.23">傷寒逐風</seg>
                    <seg xml:id="CH8x3102_CHANT_001-1a.24"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-2a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-2a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-2a.3" resp="#chris" modified="2023-01-28T15:45:15.574+09:00">治心腹大積上下行如虫狀大恿方：</seg>
                    <seg xml:id="CH8x3102_CHANT_001-2a.4"/>
                    <seg xml:id="CH8x3102_CHANT_001-2a.5">班𨱞十枚<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-2a.6"/>
                    <seg xml:id="CH8x3102_CHANT_001-2a.7" n="CH0F22EE">地<c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>一枚<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-2a.8"/>
                    <seg xml:id="CH8x3102_CHANT_001-2a.9">桂一寸<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-2a.10"/>
                    <seg xml:id="CH8x3102_CHANT_001-2a.11">凡三物皆并</seg>
                    <seg xml:id="CH8x3102_CHANT_001-2a.12"/>
                    <seg xml:id="CH8x3102_CHANT_001-2a.13">冶合和<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-2a.14"/>
                    <seg xml:id="CH8x3102_CHANT_001-2a.15">使病者宿毋食<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-2a.16"/>
                    <seg xml:id="CH8x3102_CHANT_001-2a.17">旦飲藥一刀圭<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-2a.18"/>
                    <seg xml:id="CH8x3102_CHANT_001-2a.19" n="CH0F230DCH0F230E">以肥美<c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>
                        <c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-2a.20"/>
                    <seg xml:id="CH8x3102_CHANT_001-2a.21">十日壹飲藥<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-2a.22"/>
                    <seg xml:id="CH8x3102_CHANT_001-2a.23">如有徵當出<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-2a.24"/>
                    <seg xml:id="CH8x3102_CHANT_001-2a.25">
                        <c n="、"/>從</seg>
                    <seg xml:id="CH8x3102_CHANT_001-2a.26"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-3a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-3a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-3a.3" n="CH0F2340">
                        <c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>治伏梁裏膿在胃腸之外方<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-3a.4"/>
                    <seg xml:id="CH8x3102_CHANT_001-3a.5">大黃⺀芩<c n="、"/>勺藥各一兩<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-3a.6"/>
                    <seg xml:id="CH8x3102_CHANT_001-3a.7">消石二兩<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-3a.8"/>
                    <seg xml:id="CH8x3102_CHANT_001-3a.9">桂一尺<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-3a.10"/>
                    <seg xml:id="CH8x3102_CHANT_001-3a.11"/>
                    <seg xml:id="CH8x3102_CHANT_001-3a.12">桑卑肖十四枚<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-3a.13"/>
                    <seg xml:id="CH8x3102_CHANT_001-3a.14">䗪虫三枚<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-3a.15"/>
                    <seg xml:id="CH8x3102_CHANT_001-3a.16">凡七物皆父且<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-3a.17"/>
                    <seg xml:id="CH8x3102_CHANT_001-3a.18">漬以淳酒五升<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-3a.19"/>
                    <seg xml:id="CH8x3102_CHANT_001-3a.20">卒時煮之三</seg>
                    <seg xml:id="CH8x3102_CHANT_001-3a.21"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-4a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-4a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-4a.3" n="CH0F230DCH0F230E">去中令病後不復發<c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>
                        <c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>方<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-4a.4"/>
                    <seg xml:id="CH8x3102_CHANT_001-4a.5">𥥯地長與人等<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-4a.6"/>
                    <seg xml:id="CH8x3102_CHANT_001-4a.7">深七尺<c n="、"/>橫五尺<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-4a.8"/>
                    <seg xml:id="CH8x3102_CHANT_001-4a.9">用白羊矢乾之十餘石置其</seg>
                    <seg xml:id="CH8x3102_CHANT_001-4a.10"/>
                    <seg xml:id="CH8x3102_CHANT_001-4a.11">阬中<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-4a.12"/>
                    <seg xml:id="CH8x3102_CHANT_001-4a.13">從火其上<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-4a.14"/>
                    <seg xml:id="CH8x3102_CHANT_001-4a.15">羊矢盡索橫木阬上取其卧<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-4a.16"/>
                    <seg xml:id="CH8x3102_CHANT_001-4a.17">人卧其阬上<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-4a.18"/>
                    <seg xml:id="CH8x3102_CHANT_001-4a.19">熱气盡乃止<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-4a.20"/>
                    <seg xml:id="CH8x3102_CHANT_001-4a.21">其病者慎勿得出見<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-4a.22"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-5a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-5a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-5a.3" n="CH0F2340">
                        <c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>治金創內漏血不出方藥<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-5a.4"/>
                    <seg xml:id="CH8x3102_CHANT_001-5a.5">用大黃𣍝二分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-5a.6"/>
                    <seg xml:id="CH8x3102_CHANT_001-5a.7">曾青二分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-5a.8"/>
                    <seg xml:id="CH8x3102_CHANT_001-5a.9">消石二分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-5a.10"/>
                    <seg xml:id="CH8x3102_CHANT_001-5a.11">䗪虫三分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-5a.12"/>
                    <seg xml:id="CH8x3102_CHANT_001-5a.13">䖟</seg>
                    <seg xml:id="CH8x3102_CHANT_001-5a.14"/>
                    <seg xml:id="CH8x3102_CHANT_001-5a.15">頭二分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-5a.16"/>
                    <seg xml:id="CH8x3102_CHANT_001-5a.17">凡五物皆冶合和<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-5a.18"/>
                    <seg xml:id="CH8x3102_CHANT_001-5a.19">以方寸匕一酒飲<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-5a.20"/>
                    <seg xml:id="CH8x3102_CHANT_001-5a.21">不過再飲<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-5a.22"/>
                    <seg xml:id="CH8x3102_CHANT_001-5a.23">血立出<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-5a.24"/>
                    <seg xml:id="CH8x3102_CHANT_001-5a.25">不即從大便出<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-5a.26"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-6a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-6a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-6a.3">治金創止慂方<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-6a.4"/>
                    <seg xml:id="CH8x3102_CHANT_001-6a.5">石膏一分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-6a.6"/>
                    <seg xml:id="CH8x3102_CHANT_001-6a.7">薑二分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-6a.8"/>
                    <seg xml:id="CH8x3102_CHANT_001-6a.9">甘草一分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-6a.10"/>
                    <seg xml:id="CH8x3102_CHANT_001-6a.11">桂一分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-6a.12"/>
                    <seg xml:id="CH8x3102_CHANT_001-6a.13">凡四物皆冶合和<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-6a.14"/>
                    <seg xml:id="CH8x3102_CHANT_001-6a.15">以方寸寸酢</seg>
                    <seg xml:id="CH8x3102_CHANT_001-6a.16"/>
                    <seg xml:id="CH8x3102_CHANT_001-6a.17">漿飲之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-6a.18"/>
                    <seg xml:id="CH8x3102_CHANT_001-6a.19">日再<c n="、"/>夜一<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-6a.20"/>
                    <seg xml:id="CH8x3102_CHANT_001-6a.21">良甚<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-6a.22"/>
                    <seg xml:id="CH8x3102_CHANT_001-6a.23">勿傳也<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-6a.24"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-7a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-7a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-7a.3" n="CH0F2340">
                        <c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>治金膓出方<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-7a.4"/>
                    <seg xml:id="CH8x3102_CHANT_001-7a.5">治龍骨三指撮<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-7a.6"/>
                    <seg xml:id="CH8x3102_CHANT_001-7a.7">以豉汁飲之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-7a.8"/>
                    <seg xml:id="CH8x3102_CHANT_001-7a.9">日再<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-7a.10"/>
                    <seg xml:id="CH8x3102_CHANT_001-7a.11">三飲<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-7a.12"/>
                    <seg xml:id="CH8x3102_CHANT_001-7a.13">膓自為入<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-7a.14"/>
                    <seg xml:id="CH8x3102_CHANT_001-7a.15">大良<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-7a.16"/>
                    <seg xml:id="CH8x3102_CHANT_001-7a.17">勿傳也<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-7a.18"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-8a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-8a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-8a.3" n="CH0F2340">
                        <c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>治□□□□□□潰醫不能治禁方<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-8a.4"/>
                    <seg xml:id="CH8x3102_CHANT_001-8a.5">其不𢠚者<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-8a.6"/>
                    <seg xml:id="CH8x3102_CHANT_001-8a.7">半夏<c n="、"/>白斂<c n="、"/>勺藥<c n="、"/>細辛<c n="、"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-8a.8"/>
                    <seg xml:id="CH8x3102_CHANT_001-8a.9" n="CH0F22EF">烏喙<c n="、"/>赤石脂<c n="、"/>貸赭<c n="、"/>赤豆初生未卧者<c n="、"/>
                        <c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>矢<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-8a.10"/>
                    <seg xml:id="CH8x3102_CHANT_001-8a.11">凡九物皆并冶合<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-8a.12"/>
                    <seg xml:id="CH8x3102_CHANT_001-8a.13">其分各等<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-8a.14"/>
                    <seg xml:id="CH8x3102_CHANT_001-8a.15">合和</seg>
                    <seg xml:id="CH8x3102_CHANT_001-8a.16"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-9a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.3">治千金膏藥方<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.4"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.5">蜀椒四升<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.6"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.7">弓窮一升<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.8"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.9">白芷一升<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.10"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.11">付子卅果<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.12"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.13">凡四物</seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.14"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.15">皆冶父且置銅器中<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.16"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.17" n="CH0F22F0">用淳<c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>三升漬之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.18"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.19" n="CH0F22F1">卒時取<c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>猪肪三斤<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.20"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.21">先前</seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.22"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.23">之先取鷄子中黃者置梧中撓之三百<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.24"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.25">取藥成<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.26"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.27">以五分匕一置鷄子中復</seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.28"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.29">撓之二百<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.30"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.31">薄以塗其雍者<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.32"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.33">上空者遺之中央大如錢<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.34"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.35">藥乾<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.36"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.37">復塗之如</seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.38"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.39">前法<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.40"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.41">三塗<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.42"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.43">去其故藥<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.44"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.45">其毋農者行𢠚<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.46"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.47">已有農者潰<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.48"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.49">毋得力作<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.50"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.51">禁食諸采</seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.52"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.53" n="CH0F22ED">□置□上甚創恿<c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>皆中之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.54"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.55">良<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.56"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.57">勿傳也<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.58"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.59"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.60">逆气吞之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.61"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.62">喉痹吞之摩之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.63"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.64">心腹恿吞之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.65"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.66">嗌恿吞之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.67"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.68">血府恿吞之摩之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.69"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.70">咽</seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.71"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.72">乾摩之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.73"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.74">齒恿塗之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.75"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.76" n="CH0F22F2">昏<c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>塗之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.77"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.78">鼻中生惡傷塗之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.79"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.80">亦可吞之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.81"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.82">皆大如</seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.83"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.84">酸棗<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.85"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.86">稍咽之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.87"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.88">腸中有益為度摩之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.89"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.90">皆三乾而止<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.91"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.92">此方禁<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.93"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.94">又中邚人乳餘<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.95"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.96"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.97">□吞之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.98"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.99">氣龍裹藥以縠塞之耳<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.100"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.101">日壹易之<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.102"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.103">金創塗之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.104"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.105">頭恿風塗</seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.106"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.107">之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.108"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.109">以三指摩□□□𢇢吞之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.110"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.111">身生惡氣塗之<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.112"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.113">此膏藥大良<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.114"/>
                    <seg xml:id="CH8x3102_CHANT_001-9a.115">勿得傳<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-9a.116"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-10a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-10a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-10a.3">六日脛中當恿⺀至足下傷膿出<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-10a.4"/>
                    <seg xml:id="CH8x3102_CHANT_001-10a.5">逐服之卅日<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-10a.6"/>
                    <seg xml:id="CH8x3102_CHANT_001-10a.7">知𢠚<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-10a.8"/>
                    <seg xml:id="CH8x3102_CHANT_001-10a.9">六十日須麇生<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-10a.10"/>
                    <seg xml:id="CH8x3102_CHANT_001-10a.11">音聲雖㯕敗<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-10a.12"/>
                    <seg xml:id="CH8x3102_CHANT_001-10a.13">能復精鼻柱<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-10a.14"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-11a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.3" n="CH0F2263">鼻中當<c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>血出若膿出去死肉藥<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.4"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.5">用代廬如<c n="、"/>巴豆各一分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.6"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.7">并合和<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.8"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.9">以絮裹藥塞鼻<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.10"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.11">諸息肉皆</seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.12"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.13">出<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.14"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.15">不出<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.16"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.17">更飲調中藥⺀<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.18"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.19">用亭磿二分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.20"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.21">甘遂二分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.22"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.23">大黃一分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.24"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.25">冶合和<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.26"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.27">以米汁飲一刀圭<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.28"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.29">日三<c n="、"/>四飲<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.30"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.31">徵出乃止<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.32"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.33">即鼻不利</seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.34"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.35">藥<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.36"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.37">用利廬一本<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.38"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.39">亭磨二分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.40"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.41">付子一分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.42"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.43">早莢一分<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.44"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.45">皆并父且合和<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.46"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.47">以醇醯漬<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.48"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.49">卒時去宰<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.50"/>
                    <seg xml:id="CH8x3102_CHANT_001-11a.51">以汁灌其鼻中</seg>
                    <seg xml:id="CH8x3102_CHANT_001-11a.52"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-12a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-12a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-12a.3">〼徵當下<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-12a.4"/>
                    <seg xml:id="CH8x3102_CHANT_001-12a.5">從大便出<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-12a.6"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-12a.7"/>
                    <seg xml:id="CH8x3102_CHANT_001-12a.8">〼老瘦者以人事感之<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-12a.9"/>
                    <seg xml:id="CH8x3102_CHANT_001-12a.10">此藥亦中治毒養如</seg>
                    <seg xml:id="CH8x3102_CHANT_001-12a.11"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-13a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-13a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-13a.3">〼飲食數□禁不傳也<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-13a.4"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-13a.5"/>
                    <seg xml:id="CH8x3102_CHANT_001-13a.6">〼二升□□復置水一升其中<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-13a.7"/>
                    <seg xml:id="CH8x3102_CHANT_001-13a.8">為東鄉造<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-13a.9"/>
                    <seg xml:id="CH8x3102_CHANT_001-13a.10">炊以葦薪若桑<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-13a.11"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-14a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-14a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-14a.3">〼相得<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-14a.4"/>
                    <seg xml:id="CH8x3102_CHANT_001-14a.5">丸之大如吾實<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-14a.6"/>
                    <seg xml:id="CH8x3102_CHANT_001-14a.7">先餔食吞二丸<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-14a.8"/>
                    <seg xml:id="CH8x3102_CHANT_001-14a.9">日再<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-14a.10"/>
                    <seg xml:id="CH8x3102_CHANT_001-14a.11">服藥一</seg>
                    <seg xml:id="CH8x3102_CHANT_001-14a.12"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-15a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-15a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-15a.3">〼四兩<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-15a.4"/>
                    <seg xml:id="CH8x3102_CHANT_001-15a.5">消石二兩<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-15a.6"/>
                    <seg xml:id="CH8x3102_CHANT_001-15a.7">人參<c n="、"/>方風<c n="、"/>細辛各一兩<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3102_CHANT_001-15a.8"/>
                    <seg xml:id="CH8x3102_CHANT_001-15a.9">肥棗五</seg>
                    <seg xml:id="CH8x3102_CHANT_001-15a.10"/>
                </p>
            <p>
                    <seg xml:id="CH8x3102_CHANT_001-16a.1"/>
                    <seg xml:id="CH8x3102_CHANT_001-16a.2"/>
                    <seg xml:id="CH8x3102_CHANT_001-16a.3" n="CH0F2340">
                        <c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>右治百病方</seg>
                    <seg xml:id="CH8x3102_CHANT_001-16a.4"/>
                </p>
         </div>
      </body>
   </text>
