<head xml:id="CH8x3201_CHANT_001-1a.1-h-h">
                    <seg xml:id="CH8x3201_CHANT_001-1a.1-h">武威漢簡-甲本士相見之禮釋文</seg>
                </head>
            <p>
                    <seg xml:id="CH8x3201_CHANT_001-1a.1"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.2"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.3">●士相見之禮<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.4"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.5">墊<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.6"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.7">冬用雉<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.8"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.9">夏用居<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.10"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.11">左梪奉之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.12"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.13">曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.14"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.15">
                        <c n="「"/>某也願見<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.16"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.17">無由達<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.18"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.19">某子以命命某見<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.20"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.21">主人𡭊曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.22"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.23">
                        <c n="「"/>某子以命命某見<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.24"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.25">吾子又辱<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.26"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.27">請吾子之就家<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.28"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.29">某將走見<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.30"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.31">賓對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.32"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.33">
                        <c n="「"/>某</seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.34"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.35"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.36">不足以辱命<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.37"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.38">請終賜見<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.39"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.40">主人對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.41"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.42">
                        <c n="「"/>某非敢為儀<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.43"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.44">請吾子之就家<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.45"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.46">某將走見<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.47"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.48">賓對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.49"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.50">
                        <c n="「"/>某非敢為儀<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.51"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.52">固以請<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.53"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.54">主人對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.55"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.56">
                        <c n="「"/>某固辤不得命<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.57"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.58">將走見<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.59"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.60">聞吾子稱執<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.61"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.62">敢辤墊<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.63"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.64"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.65">賓對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.66"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.67">
                        <c n="「"/>某不以墊<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.68"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.69">不敢見<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.70"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.71">主人對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.72"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.73">
                        <c n="「"/>某不足以習禮<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.74"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.75">敢固辤<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.76"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.77">賓對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.78"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.79">
                        <c n="「"/>某不依于墊<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.80"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.81">不敢見<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.82"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.83">敢固以請<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.84"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.85">主人對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.86"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.87">
                        <c n="「"/>某固辤<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.88"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.89">不得命<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.90"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.91">敢不從<c n="！"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.92"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.93">出迎再拜<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.94"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.95">賓合拜<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.96"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.97">主人</seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.98"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.99">揖<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.100"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.101">入門右<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.102"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.103">賓奉墊<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.104"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.105">入門左<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.106"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.107">主人拜受<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.108"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.109">賓拜送墊<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.110"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.111">出<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.112"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.113">主人請見<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.114"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.115">賓反見<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.116"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.117">退<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.118"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.119">主人送<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.120"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.121">再拜<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.122"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.123">復見之<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.124"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.125">以某墊<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.126"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.127">曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.128"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.129">
                        <c n="「"/>鄉者吾子辱使某見<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.130"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.131">請還墊於將命者<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.132"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.133">主人對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.134"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.135">
                        <c n="「"/>某既</seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.136"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.137">得見矣<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.138"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.139">敢辤<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.140"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.141">對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.142"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.143">
                        <c n="「"/>某非敢求見<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.144"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.145">請還墊於將命者<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.146"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.147">主人對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.148"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.149">
                        <c n="「"/>某既得見矣<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.150"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.151">敢固辭<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.152"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.153">賓對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.154"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.155">
                        <c n="「"/>某非敢以聞<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.156"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.157">固以請於將命者<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.158"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.159">主人對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.160"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.161">
                        <c n="「"/>某固辤<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.162"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.163">不得命<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.164"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.165">敢不從<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.166"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.167">賓</seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.168"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.169">奉墊入<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.170"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.171">主人拜受<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.172"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.173">賓拜送墊<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.174"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.175">出<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.176"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.177">主人送于門外<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.178"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.179">再拜<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.180"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.181">
                        <c n="‧"/>士見於大夫<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.182"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.183">終辤其墊<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.184"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.185">於其入也<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.186"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.187">壹拜其辱也<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.188"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.189">賓退<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.190"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.191">再拜<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.192"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.193">如當為臣者<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.194"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.195">則禮辤其墊<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.196"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.197">賓入鄭墊再拜<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.198"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.199">主人</seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.200"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.201">合壹拜<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.202"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.203">賓出<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.204"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.205">使擯者還其墊於門外<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.206"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.207">曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.208"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.209">
                        <c n="「"/>某也<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.210"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.211">使某還墊<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.212"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.213">賓對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.214"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.215">
                        <c n="「"/>某既得見矣<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.216"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.217">敢辤<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.218"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.219">賓擯者曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.220"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.221">
                        <c n="『"/>某使某非敢為儀<c n="』"/>固以請<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.222"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.223">賓對曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.224"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.225">
                        <c n="「"/>某夫子之賤私也<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.226"/>
