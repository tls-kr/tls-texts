                    <seg xml:id="CH8x3201_CHANT_001-1a.471">三辤<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.472"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.473">
                        <c n="‧"/>如先生與</seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.474"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.475">爵者請見之則辤<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.476"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.477">不得命則曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.478"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.479">
                        <c n="「"/>某無以見<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.480"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.481">辤不得命<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.482"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.483">將走見<c n="。"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.484"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.485">先見之<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.486"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.487">
                        <c n="‧"/>非以君命使<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.488"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.489">則不稱寫大夫則曰寡君之恭<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.490"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.491">凡執敝<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.492"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.493">執敝者<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.494"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.495">不</seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.496"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.497">趨<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.498"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.499">容彌蹙以為儀<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.500"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.501">執玉</seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.502"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.503">者<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.504"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.505">唯</seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.506"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.507" n="CH0F2001">
                        <c n="&amp;"/>
                        <c n="-"/>
                        <c n=";"/>𢦶舉前肆踵<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.508"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.509">凡自稱於君<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.510"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.511">士大夫曰<c n="「"/>下臣<c n="」"/>
                        <c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.512"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.513">詫者在國則曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.514"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.515">
                        <c n="「"/>市井之臣<c n="」"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.516"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.517">在野則曰<c n="："/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.518"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.519">
                        <c n="「"/>草茅之臣<c n="」"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.520"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.521">庶人則曰<c n="「"/>刺草之臣也<c n="」"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.522"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.523">國之人則曰<c n="「"/>外臣<c n="」"/>
                        <c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.524"/>
                    <seg xml:id="CH8x3201_CHANT_001-1a.525">
                        <c n="‧"/>凡千二十字<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x3201_CHANT_001-1a.526"/>
                </p>
         </div>
      </body>
   </text>
</TEI>