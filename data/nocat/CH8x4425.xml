                    <seg xml:id="CH8x4425_CHANT_001-1a.63">凡悔<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.64">已道者也<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.65">凡咎<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.66">有不行者也<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.67">凡過正一<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.68">以失其它</seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.69">者也<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.70">凡悅<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.71">作於譽者也</seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.72">華<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.73">自安也<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.74">惻<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.75">退人也<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.76">名<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.77">數也<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.78">由占鯀生<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.79">未有善事人而不返者<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.80">未有嘩而忠者<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.81">知命者亡咎<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.82">有德者不移<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.83">疑取再<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.84">毋失吾勢<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.85">此勢得矣<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.86">小不忍<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.87">敗大勢<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.88">其所之同<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.89">其所者異<c n="。"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.90">有行而不由<c n="，"/>
                    </seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.91">有由而</seg>
                    <seg xml:id="CH8x4425_CHANT_001-1a.92">不行<c n="。"/>
