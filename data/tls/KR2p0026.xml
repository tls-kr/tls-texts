<p source="GUODIAN XINGZIMINGCHU 13.27. SLIPS 37-38">
                    <pb xml:id="KR2p0026_tls_001-28a"/>
                    <seg xml:id="KR2p0026_tls_001-28a.1">求其心</seg>
                    <seg xml:id="KR2p0026_tls_001-28a.2">有偽也<c n="，"/>
