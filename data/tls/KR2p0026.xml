<text>
<body>
<div>

<p source="GUODIAN XINGZIMINGCHU 13.1. SLIPS 1-2">
                    <pb xml:id="KR2p0026_tls_001-2a"/>
                    <seg xml:id="KR2p0026_tls_001-2a.1">凡人雖有性<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-2a.2">心亡定志<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-2a.3" n="TZL">待<c n="["/>恃<c n="]"/>物</seg>
                    <seg xml:id="KR2p0026_tls_001-2a.4">而後作<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-2a.5" n="TZL">待<c n="["/>恃<c n="]"/>悅</seg>
                    <seg xml:id="KR2p0026_tls_001-2a.6">而後行<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-2a.7" n="TZL">待<c n="["/>恃<c n="]"/>習</seg>
                    <seg xml:id="KR2p0026_tls_001-2a.8">而後定<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.2. SLIPS 2-3">
                    <pb xml:id="KR2p0026_tls_001-3a"/>
                    <seg xml:id="KR2p0026_tls_001-3a.1">喜怒哀悲之氣<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-3a.2">性也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-3a.3">及其見於外<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-3a.4">則物取之也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-3a.5">性自命出<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-3a.6">命自天降<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-3a.7">道始於情<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-3a.8">情生於性<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-3a.9">始者近情<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-3a.10">終者近義<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.3. SLIPS 3-5">
                    <pb xml:id="KR2p0026_tls_001-4a"/>
                    <seg xml:id="KR2p0026_tls_001-4a.1">知情<c n="〔"/>者能<c n="〕"/>出之<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-4a.2">知義者能入之<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-4a.3">好惡<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-4a.4">性也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-4a.5">所好所惡<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-4a.6">物也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-4a.7">善<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-4a.8">不<c n="〔"/>善<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-4a.9">性也<c n="〕"/>
                        <c n=","/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-4a.10">所善</seg>
                    <seg xml:id="KR2p0026_tls_001-4a.11">所不善<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-4a.12">勢也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.4. SLIPS 5-6">
                    <pb xml:id="KR2p0026_tls_001-5a"/>
                    <seg xml:id="KR2p0026_tls_001-5a.1">凡性為主<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-5a.2">物取之也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-5a.3" n="SM">金石之有聲<c n="["/>也<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-5a.4">弗扣</seg>
                    <seg xml:id="KR2p0026_tls_001-5a.5">不鳴<c n="]"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-5a.6">雖有性<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-5a.7">心弗取</seg>
                    <seg xml:id="KR2p0026_tls_001-5a.8">不出<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.5. SLIPS 6-8">
                    <pb xml:id="KR2p0026_tls_001-6a"/>
                    <seg xml:id="KR2p0026_tls_001-6a.1">凡心有志也<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-6a.2" n="DYZ">亡舉<c n="["/>與<c n="]"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-6a.3">不<c n="〔"/>可<c n="〕"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-6a.4">
                        <c n="〔"/>人之不可<c n="〕"/>獨行<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-6a.5">猶口之不可獨言也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-6a.6">牛生而長<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-6a.7">雁生而伸<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-6a.8" n=".">其性<c n="〔"/>使然<c n="〕"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-6a.9">
                        <c n="〔"/>人<c n="〕"/>而學<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-6a.10">或使之也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.6. SLIPS 8-9">
                    <pb xml:id="KR2p0026_tls_001-7a"/>
                    <seg xml:id="KR2p0026_tls_001-7a.1">凡物亡不異也者<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-7a.2" n="LL">剛之柱<c n="["/>樹<c n="]"/>也<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-7a.3">剛取之也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-7a.4">柔之約<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-7a.5">柔取之也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-7a.6">四海之內其性一也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-7a.7">其用心各異<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-7a.8">教使然也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.7. SLIPS 9-10">
                    <pb xml:id="KR2p0026_tls_001-8a"/>
                    <seg xml:id="KR2p0026_tls_001-8a.1">凡性</seg>
                    <seg xml:id="KR2p0026_tls_001-8a.2">或動之<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-8a.3">或逆之<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-8a.4">或交之<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-8a.5">或礪之<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-8a.6">或出之<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-8a.7">或養之<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-8a.8">或長之<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.8. SLIPS 10-12">
                    <pb xml:id="KR2p0026_tls_001-9a"/>
                    <seg xml:id="KR2p0026_tls_001-9a.1">凡動性者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-9a.2">物也<c n="；"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-9a.3">逆性者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-9a.4">悅也<c n="；"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-9a.5">交性者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-9a.6">故也<c n="；"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-9a.7">礪性者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-9a.8">義也<c n="；"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-9a.9">出性者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-9a.10">勢也<c n="；"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-9a.11">養性 者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-9a.12">習也<c n="；"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-9a.13">長性者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-9a.14">道也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.9. SLIPS 12-14">
                    <pb xml:id="KR2p0026_tls_001-10a"/>
                    <seg xml:id="KR2p0026_tls_001-10a.1">凡見者之謂物<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-10a.2">快於己者之謂悅<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-10a.3">物之勢者之謂勢<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-10a.4">有為也者之謂故<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-10a.5">義也者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-10a.6">群善之蕝也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-10a.7">習也者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-10a.8">有以習其性也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-10a.9">道者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-10a.10">群物之道<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.10. SLIPS 14-15">
                    <pb xml:id="KR2p0026_tls_001-11a"/>
                    <seg xml:id="KR2p0026_tls_001-11a.1">凡道<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-11a.2">心術為主<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-11a.3">道四術<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-11a.4">唯人道</seg>
                    <seg xml:id="KR2p0026_tls_001-11a.5">為可道也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-11a.6">其三術者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-11a.7">道之而已<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.11. SLIPS 15-16">
                    <pb xml:id="KR2p0026_tls_001-12a"/>
                    <seg xml:id="KR2p0026_tls_001-12a.1">詩書禮樂<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-12a.2">其始出皆生 於人<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-12a.3">詩<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-12a.4">有為<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-12a.5" n="SHR1168.6">為 <c n="["/>哦<c n=":"/>  n <c n="]"/> 之也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-12a.6">書<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-12a.7">有為<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-12a.8">言之也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-12a.9">禮樂<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-12a.10">有為<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-12a.11">舉之也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.12. SLIPS 16-18">
                    <pb xml:id="KR2p0026_tls_001-13a"/>
                    <seg xml:id="KR2p0026_tls_001-13a.1">聖人比其類</seg>
                    <seg xml:id="KR2p0026_tls_001-13a.2">而論會之<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-13a.3">觀其先後</seg>
                    <seg xml:id="KR2p0026_tls_001-13a.4">而逆訓之<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-13a.5">體其義</seg>
                    <seg xml:id="KR2p0026_tls_001-13a.6" n="QXGLL">而節<c n="("/>次<c n=")"/>文<c n="("/>度<c n=")"/>之<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-13a.7">理其情</seg>
                    <seg xml:id="KR2p0026_tls_001-13a.8">而出入之<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-13a.9">然後復以教<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.13. SLIPS 18-19">
                    <pb xml:id="KR2p0026_tls_001-14a"/>
                    <seg xml:id="KR2p0026_tls_001-14a.1">教<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-14a.2">所以生德于中者也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-14a.3">禮<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-14a.4">作於情<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-14a.5" n="QXG">或興<c n="("/>遷<c n=")"/>之也<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-14a.6">當事</seg>
                    <seg xml:id="KR2p0026_tls_001-14a.7">因方</seg>
                    <seg xml:id="KR2p0026_tls_001-14a.8">而制之<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-14a.9">其先後之序</seg>
                    <seg xml:id="KR2p0026_tls_001-14a.10" n="DYZ">則宜<c n="["/>義<c n="]"/>道也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.14. SLIPS 19-21">
                    <pb xml:id="KR2p0026_tls_001-15a"/>
                    <seg xml:id="KR2p0026_tls_001-15a.1" n="LL">或<c n="["/>有<c n="]"/>序<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-15a.2">為之節</seg>
                    <seg xml:id="KR2p0026_tls_001-15a.3" n="QXG">則文<c n="("/>度<c n=")"/>也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-15a.4">致容貌<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-15a.5" n="QXG">所以文<c n="("/>度<c n=")"/>
                        <c n=","/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-15a.6">節也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-15a.7">君子美其情<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-15a.8">貴<c n="〔"/>其義<c n="〕"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-15a.9">善其節<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-15a.10">好其容<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-15a.11">樂其道<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-15a.12">悅其教<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-15a.13">是以敬焉<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.15. SLIPS 21-23">
                    <pb xml:id="KR2p0026_tls_001-16a"/>
                    <seg xml:id="KR2p0026_tls_001-16a.1">拜<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-16a.2" n="DYZ.">所以<c n="〔"/>為服也<c n="〕"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-16a.3" n="XSHR1173.18DYZ">其 <c n="["/>數<c n="("/>=禮<c n=")"/>
                        <c n=":"/> n<c n="]"/>
                        <c n="("/>諛<c n=")"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-16a.4" n="QXG">文<c n="("/>度<c n=")"/>也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-16a.5">幣帛<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-16a.6">所以為信與徵也<c n=";"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-16a.7" n="SHR1174.19">其詞<c n="["/>貽<c n=":"/>  n <c n="]"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-16a.8" n="DYZ">宜<c n="["/>義<c n="]"/> 道也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-16a.9">笑<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-16a.10">禮之淺澤也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-16a.11">樂<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-16a.12">禮之深澤也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.16. SLIP 23">
                    <pb xml:id="KR2p0026_tls_001-17a"/>
                    <seg xml:id="KR2p0026_tls_001-17a.1">凡聲<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-17a.2" resp="#CH" modified="2023-10-01T19:37:29.939+09:00">其出於情也信，</seg>
                    <seg xml:id="KR2p0026_tls_001-17a.4" resp="#CH" modified="2023-10-01T19:39:06.072+09:00">LL　然後其入拔[撥]人之心也厚.</seg>
                    <seg xml:id="KR2p0026_tls_001-17a.5" resp="#CH" modified="2023-10-01T19:38:24.573+09:00"/>
                </p>

<p source="GUODIAN XINGZIMINGCHU 13.17. SLIPS 24-25">
                    <pb xml:id="KR2p0026_tls_001-18a"/>
                    <seg xml:id="KR2p0026_tls_001-18a.1">聞笑聲<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-18a.2">則鮮如也</seg>
                    <seg xml:id="KR2p0026_tls_001-18a.3">斯喜<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-18a.4">聞歌謠<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-18a.5">則陶如也</seg>
                    <seg xml:id="KR2p0026_tls_001-18a.6">斯奮<c n="。"/>
                    </seg>
