                    <seg xml:id="KR2p0026_tls_001-31a.1">凡用心之躁者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-31a.2">思為甚<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-31a.3">用智之疾者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-31a.4">患為甚<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-31a.5">用情之至者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-31a.6">哀樂為甚<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0026_tls_001-31a.7" n="LL">用身之弁<c n="["/>便<c n="]"/>者<c n="，"/>
