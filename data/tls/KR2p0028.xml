<body>
<div>

<p source="GUODIAN, YUCONG 1, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-1a"/>
                    <seg xml:id="KR2p0028_tls_001-1a.1">凡物由亡生<c n="。"/>　<c n="("/>亡 written as 望<c n=")"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.2, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-2a"/>
                    <seg xml:id="KR2p0028_tls_001-2a.1">天生倫<c n="("/>
                        <c n="?"/>
                        <c n=")"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-2a.2">人生化<c n="("/>
                        <c n="?"/>
                        <c n=")"/>
                        <c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.3, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-3a"/>
                    <seg xml:id="KR2p0028_tls_001-3a.1">有天有命<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-3a.2">有地有形<c n="；"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-3a.3">有物有容<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-3a.4">有盡有厚<c n="。"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-3a.5">有地有形<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-3a.6">有盡而後<c n="／"/>有厚<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.4, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-4a"/>
                    <seg xml:id="KR2p0028_tls_001-4a.1">有天有命<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-4a.2">有物有名<c n="；"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-4a.3">有物有容<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-4a.4">有稱有名<c n="。"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-4a.5">有命有文<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-4a.6">有名而後有倫<c n="("/>
                        <c n="?"/>
                        <c n=")"/>
                        <c n="。"/>
                        <c n="/"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.5, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-5a"/>
                    <seg xml:id="KR2p0028_tls_001-5a.1">有物有由有尾<c n="（"/>
                        <c n="？"/>
                        <c n="）"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-5a.2">而後教生<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.6, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-6a"/>
                    <seg xml:id="KR2p0028_tls_001-6a.1">有生有知<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-6a.2">而後好惡<c n="/"/>生<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.7, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-7a"/>
                    <seg xml:id="KR2p0028_tls_001-7a.1">有仁有知<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-7a.2">有義有禮<c n="，"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-7a.3">有美有善<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.8, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-8a"/>
                    <seg xml:id="KR2p0028_tls_001-8a.1">有聖有善<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.9, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-9a"/>
                    <seg xml:id="KR2p0028_tls_001-9a.1">天生百物<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-9a.2">人為貴<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-9a.3">人<c n="/"/>之道也<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-9a.4">或<c n="/"/>由中出<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-9a.5">或由外入<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.10, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-10a"/>
                    <seg xml:id="KR2p0028_tls_001-10a.1">由中出者<c n="："/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-10a.2">仁忠信<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-10a.3">由<c n="【"/>外入者<c n="："/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-10a.4">禮樂刑<c n="。"/>
                        <c n="】"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.12, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-11a"/>
                    <seg xml:id="KR2p0028_tls_001-11a.1">仁義為之臬<c n="("/>
                        <c n="?"/>
                        <c n=")"/>
                        <c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.11, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-12a"/>
                    <seg xml:id="KR2p0028_tls_001-12a.1">愛善之謂仁<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.13, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-13a"/>
                    <seg xml:id="KR2p0028_tls_001-13a.1">喪<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-13a.2">仁之端也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.14, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-14a"/>
                    <seg xml:id="KR2p0028_tls_001-14a.1">上下皆得其所之謂信<c n="。"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-14a.2">信非至齊也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.15, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-15a"/>
                    <seg xml:id="KR2p0028_tls_001-15a.1">其生也亡為乎<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-15a.2">其刑<c n="/"/>生德<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-15a.3">德生禮<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-15a.4">禮生樂<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-15a.5">由樂<c n="/"/>知刑<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-15a.6">知禮然後知刑<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.16, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-16a"/>
                    <seg xml:id="KR2p0028_tls_001-16a.1">知禮然後知刑<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.17, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-17a"/>
                    <seg xml:id="KR2p0028_tls_001-17a.1">刑非嚴也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.18, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-18a"/>
                    <seg xml:id="KR2p0028_tls_001-18a.1">禮<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-18a.2">因人之情而為之<c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-18a.3">節文者也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.19, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-19a"/>
                    <seg xml:id="KR2p0028_tls_001-19a.1">善理而後樂生<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.20, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-20a"/>
                    <seg xml:id="KR2p0028_tls_001-20a.1">賢者能理之<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.21, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-21a"/>
                    <seg xml:id="KR2p0028_tls_001-21a.1">禮生於莊<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-21a.2">樂生於度<c n="。"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-21a.3">禮齊樂靈<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-21a.4">則戚<c n="；"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-21a.5">樂繁<c n="("/>
                        <c n="?"/>
                        <c n=")"/>
                        <c n="/"/>禮靈<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-21a.6">則慢<c n="("/>
                        <c n="?"/>
                        <c n=")"/>
                        <c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.22, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-22a"/>
                    <seg xml:id="KR2p0028_tls_001-22a.1">樂x<c n="("/>
                        <c n="?"/>
                        <c n="?"/>
                        <c n=")"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.23, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-23a"/>
                    <seg xml:id="KR2p0028_tls_001-23a.1">禮不同<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-23a.2">不豐不殺<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.24, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-24a"/>
                    <seg xml:id="KR2p0028_tls_001-24a.1">知己而後知人<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-24a.2">知人而後<c n="／"/>知禮<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-24a.3">知禮而後知行<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.25, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-25a"/>
                    <seg xml:id="KR2p0028_tls_001-25a.1">知天所為<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-25a.2">知人所為<c n="，"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-25a.3">然後知道<c n="；"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-25a.4">知道然後知命<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-25a.5">其智博然後知命<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.26, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-26a"/>
                    <seg xml:id="KR2p0028_tls_001-26a.1">詩<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-26a.2">所以會古今之志<c n="/"/>也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-26a.3">書<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-26a.4" n="...">
                        <c n="【"/>所以會<c n="】"/>者也<c n="。"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-26a.5">禮<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-26a.6">交之行述也<c n="。"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-26a.7">樂<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-26a.8">或生或教者也<c n="。"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-26a.9">易<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-26a.10">所以會天道人道<c n="/"/>也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-26a.11">春秋<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-26a.12">所以會古今之<c n="/"/>事也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.27, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-27a"/>
                    <seg xml:id="KR2p0028_tls_001-27a.1">察天道以化民氣<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.28, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-28a"/>
                    <seg xml:id="KR2p0028_tls_001-28a.1">凡有血氣者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-28a.2">皆有喜<c n="/"/>有怒<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-28a.3">有慎有莊<c n="("/>
                        <c n="?"/>
                        <c n=")"/>
                        <c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-28a.4">其體<c n="/"/>有容<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-28a.5">有色有聲<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-28a.6">有嗅<c n="/"/>有味<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-28a.7">有氣有志<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-28a.8">凡物<c n="/"/>有本有化<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-28a.9">有終有始<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.29, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-29a"/>
                    <seg xml:id="KR2p0028_tls_001-29a.1">容色目司也<c n="；"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-29a.2">聲耳司<c n="/"/>也<c n="；"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-29a.3">嗅鼻司也<c n="；"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-29a.4">味口司<c n="/"/>也<c n="；"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-29a.5">氣容司也<c n="；"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-29a.6">志心司<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.30, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-30a"/>
                    <seg xml:id="KR2p0028_tls_001-30a.1">為孝<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-30a.2">此非孝也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-30a.3">為悌<c n="，"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-30a.4">此非悌也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-30a.5">不可為也<c n="，"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-30a.6">而不可不為也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-30a.7">為之<c n="，"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-30a.8">此非也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-30a.9">弗為<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-30a.10">此非也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-30a.11">義亡能為也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-30a.12">人亡能為<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.31, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-31a"/>
                    <seg xml:id="KR2p0028_tls_001-31a.1">人亡能為<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.32, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-32a"/>
                    <seg xml:id="KR2p0028_tls_001-32a.1" n="..."/>
                    <seg xml:id="KR2p0028_tls_001-32a.2">者義<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-32a.3">然不然<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.33, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-33a"/>
                    <seg xml:id="KR2p0028_tls_001-33a.1">正其然而行<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-33a.2">治焉爾也<c n="。"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-33a.3">正不達文<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-33a.4">生乎不達<c n="/"/>其然也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-33a.5">學<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-33a.6">學其<c n="（"/>＝斯<c n="？"/><c n="？"/><c n="）"/>也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.34, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-34a"/>
                    <seg xml:id="KR2p0028_tls_001-34a.1">當其然而行<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-34a.2">治焉<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.35, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-35a"/>
                    <seg xml:id="KR2p0028_tls_001-35a.1">義亡能為也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.36, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-36a"/>
                    <seg xml:id="KR2p0028_tls_001-36a.1">父子<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-36a.2">識上下也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-36a.3">兄弟<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-36a.4">識先後也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.37, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-37a"/>
                    <seg xml:id="KR2p0028_tls_001-37a.1">兄弟<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-37a.2">識先後也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.38, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-38a"/>
                    <seg xml:id="KR2p0028_tls_001-38a.1">亡物不物<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-38a.2">皆至焉<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-38a.3">而<c n="/"/>亡非己取之者<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.39, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-39a"/>
                    <seg xml:id="KR2p0028_tls_001-39a.1">仁生於人<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-39a.2">義生於道<c n="。"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-39a.3">或生於內<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-39a.4">或生於外<c n="。"/>
                        <c n="/"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.40, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-40a"/>
                    <seg xml:id="KR2p0028_tls_001-40a.1">
                        <c n="["/>厚於仁<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-40a.2">薄<c n="]"/>於義<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-40a.3">親而<c n="/"/>不尊<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-40a.4">厚於義<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-40a.5">薄於仁<c n="，"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-40a.6">尊而不親<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.41, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-41a"/>
                    <seg xml:id="KR2p0028_tls_001-41a.1">
                        <c n="【"/>別君<c n="】"/> 父<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-41a.2">有親有尊<c n="。"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-41a.3">長悌<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-41a.4">親道也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-41a.5">友<c n="，"/>君臣<c n="，"/>
                        <c n="/"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-41a.6">無親也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.42, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-42a"/>
                    <seg xml:id="KR2p0028_tls_001-42a.1">君臣<c n="、"/>朋友<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-42a.2">其擇者也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.43, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-43a"/>
                    <seg xml:id="KR2p0028_tls_001-43a.1">賓客<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-43a.2">清廟之文也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.44, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-44a"/>
                    <seg xml:id="KR2p0028_tls_001-44a.1">有察善<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-44a.2">亡為善<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.45, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-45a"/>
                    <seg xml:id="KR2p0028_tls_001-45a.1">察所知<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-45a.2">察所不知<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.46, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-46a"/>
                    <seg xml:id="KR2p0028_tls_001-46a.1">勢與聲為可察也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.47, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-47a"/>
                    <seg xml:id="KR2p0028_tls_001-47a.1">備之謂聖<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-47a.2">or<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-47a.3">服之謂聽<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.48, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-48a"/>
                    <seg xml:id="KR2p0028_tls_001-48a.1">盈<c n="("/>
                        <c n="?"/>
                        <c n=")"/>聖之謂聖<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.49, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-49a"/>
                    <seg xml:id="KR2p0028_tls_001-49a.1">決<c n="("/>or<c n=":"/>缺<c n=")"/>生乎未得<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.50, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-50a"/>
                    <seg xml:id="KR2p0028_tls_001-50a.1">有生乎名<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.51, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-51a"/>
                    <seg xml:id="KR2p0028_tls_001-51a.1">決與信<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-51a.2">器也<c n="。"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-51a.3">各以譫<c n="/"/>詞毀也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.52, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-52a"/>
                    <seg xml:id="KR2p0028_tls_001-52a.1">號與容與<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-52a.2">夫其行者<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.53, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-53a"/>
                    <seg xml:id="KR2p0028_tls_001-53a.1">食邪色邪<c n="，"/>疾<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.54, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-54a"/>
                    <seg xml:id="KR2p0028_tls_001-54a.1">悲亡其所也<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-54a.2">亡非是<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.55, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-55a"/>
                    <seg xml:id="KR2p0028_tls_001-55a.1" n="...">之弗也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.56, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-56a"/>
                    <seg xml:id="KR2p0028_tls_001-56a.1">
                        <c n="。"/>
                        <c n="。"/>
                        <c n="。"/>者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-56a.2">御寇不逮<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-56a.3">從一道<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.57, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-57a"/>
                    <seg xml:id="KR2p0028_tls_001-57a.1">多好者<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-57a.2">亡好者也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.58, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-58a"/>
                    <seg xml:id="KR2p0028_tls_001-58a.1">數<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-58a.2">不盡也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.59 ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-59a"/>
                    <seg xml:id="KR2p0028_tls_001-59a.1">詩<c n="("/>
                        <c n="?"/>
                        <c n=")"/>由敬作<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.60, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-60a"/>
                    <seg xml:id="KR2p0028_tls_001-60a.1">求者</seg>
                    <seg xml:id="KR2p0028_tls_001-60a.2">無有自來也<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.61, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-61a"/>
                    <seg xml:id="KR2p0028_tls_001-61a.1">權<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-61a.2">可去<c n="，"/>可歸<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.62, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-62a"/>
                    <seg xml:id="KR2p0028_tls_001-62a.1">凡同者通<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.63, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-63a"/>
                    <seg xml:id="KR2p0028_tls_001-63a.1">物各止於其所<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-63a.2" n="...">我行</seg>
                </p>

<p source="GUODIAN, YUCONG 1.64, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-64a"/>
                    <seg xml:id="KR2p0028_tls_001-64a.1" n="...">皆有之<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.65, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-65a"/>
                    <seg xml:id="KR2p0028_tls_001-65a.1">止之<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.66, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-66a"/>
                    <seg xml:id="KR2p0028_tls_001-66a.1">凡物由亡生<c n="。"/>
                    </seg>
                </p>

<p source="GUODIAN, YUCONG 1.67, ed. Liu Zhao 2003, ">
                    <pb xml:id="KR2p0028_tls_001-67a"/>
                    <seg xml:id="KR2p0028_tls_001-67a.1">
                        <c n="【"/>性生仁<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-67a.2">仁生忠<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-67a.3">忠生信<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-67a.4">信<c n="】"/>生德<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-67a.5">德生禮<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-67a.6">禮生樂<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0028_tls_001-67a.7">由樂<c n="/"/>知刑<c n="。"/>
                    </seg>
                </p>

</div>
</body>
