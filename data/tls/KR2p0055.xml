<p source="SHANGBO.DIZIWEN 5.26.1 (STRIPS 2, 1) MACHENGYUAN 5: 268 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-1a"/>
                    <seg xml:id="KR2p0055_tls_001-1a.1">子曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-1a.2">延陵季子<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-1a.3">其天民也乎<c n="!"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-1a.4">生</seg>
                    <seg xml:id="KR2p0055_tls_001-1a.5" n=".">而不因其俗</seg>
                    <seg xml:id="KR2p0055_tls_001-1a.6">吳人生七年</seg>
                    <seg xml:id="KR2p0055_tls_001-1a.7" n="2">
                        <c n="【"/>
                        <c n="】"/>而動散<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-1a.8">文乎其膺 <c n="("/>胸<c n=")"/>
                        <c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-1a.9">延陵季子僑<c n="("/>矯<c n=")"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-1a.10" n=".">而弗受</seg>
                    <seg xml:id="KR2p0055_tls_001-1a.11">延陵季子<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-1a.12">其天民也乎<c n="!"/>
                    </seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.2 (STRIPS 1, 23,7) MACHENGYUAN 5: 268, 281, 272 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-2a"/>
                    <seg xml:id="KR2p0055_tls_001-2a.1">子貢 <c n="["/>曰<c n=":"/>
                        <c n="]"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-2a.2" n="1......">
                        <c n="【"/>
                        <c n="】"/>
                        <c n="["/>
                        <c n="]"/> 何<c n="?"/> <c n="["/>
                        <c n="]"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-2a.3" n=".">仁之有<c n="”"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-2a.4">子曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-2a.5">
                        <c n="“"/>列乎其下<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-2a.6">不折其枝<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-2a.7" n="23">飤其實<c n="【"/>
                        <c n="】"/> <c n="["/>者<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-2a.8" n="...">不毀其器<c n="]"/> <c n="["/>
                        <c n="]"/>
                    </seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.3 (STRIPS 7, 8) MACHENGYUAN 5: 272 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-3a"/>
                    <seg xml:id="KR2p0055_tls_001-3a.1">子曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-3a.2">
                        <c n="“"/>吾聞父母之喪<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-3a.3" n="7">
                        <c n="【"/>
                        <c n="】"/>食肉如飯土<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-3a.4">飲酒如淆<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-3a.5">信乎<c n="?"/>
                        <c n="”"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-3a.6">子貢曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-3a.7">
                        <c n="“"/>莫親乎父母<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-3a.8">死不顧<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-3a.9">生可言乎<c n="?"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-3a.10" n=".">其信也</seg>
                    <seg xml:id="KR2p0055_tls_001-3a.11" n="8">子<c n="【"/>
                        <c n="】"/>
                    </seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.4 (STRIP 10) MACHENGYUAN 5: 273 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-4a"/>
                    <seg xml:id="KR2p0055_tls_001-4a.1" n="...">
                        <c n="["/>
                        <c n="]"/> 汝弗知也呼<c n="?"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-4a.2">由<c n="!"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-4a.3">夫以衆犯難<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-4a.4">以親受祿<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-4a.5">勞以成事<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-4a.6">印以擅 <c n="["/>樹<c n="]"/>官<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-4a.7">士治 <c n="["/>鉤<c n="]"/>以力</seg>
                    <seg xml:id="KR2p0055_tls_001-4a.8" n="10">則俎以<c n="【"/>
                        <c n="】"/>
                    </seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.5 (STRIPS 17, 20) MACHENGYUAN 5: 277, 279 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-5a"/>
                    <seg xml:id="KR2p0055_tls_001-5a.1" n="...">
                        <c n="["/>
                        <c n="]"/> 弗王<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-5a.2">善矣<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-5a.3">夫焉能王人<c n="?"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-5a.4">由<c n="!"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-5a.5">子過曹<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-5a.6" n="17.">
                        <c n="["/>顏<c n="]"/>
                        <c n="【"/>
                        <c n="】"/>淵馭</seg>
                    <seg xml:id="KR2p0055_tls_001-5a.7">至老丘<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-5a.8">有戎<c n="("/>農<c n=")"/>植其槈</seg>
                    <seg xml:id="KR2p0055_tls_001-5a.9" n=".">而歌焉</seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.6 (STRIPS 20, 4) MACHENGYUAN 5: 279, 269 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-6a"/>
                    <seg xml:id="KR2p0055_tls_001-6a.1" n="...20">子據乎軾而<c n="["/>
                        <c n="]"/>
                        <c n="【"/>
                        <c n="】"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-6a.2" n="...">
                        <c n="["/>
                        <c n="]"/> 風也<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-6a.3">亂節</seg>
                    <seg xml:id="KR2p0055_tls_001-6a.4" n=".">而哀聲</seg>
                    <seg xml:id="KR2p0055_tls_001-6a.5">曹之喪<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-6a.6">其必此乎<c n="?"/>
                        <c n="”"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-6a.7">
                        <c n="“"/>回<c n="!"/>
                        <c n="”"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-6a.8">子嘆曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-6a.9">
                        <c n="“"/>烏<c n="!"/>莫我知也夫<c n="!"/>
                        <c n="”"/>
                    </seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.7 (STRIPS 4, 21, 22) MACHENGYUAN 5: 269 , 280 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-7a"/>
                    <seg xml:id="KR2p0055_tls_001-7a.1">子遊曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-7a.2">
                        <c n="“"/>有地<c n="["/>施<c n="]"/>之謂也乎<c n="?"/>
                        <c n="”"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-7a.3">子曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-7a.4" n="4">偃<c n="【"/>
                        <c n="】"/>吾未見華<c n="["/>邦<c n="]"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-7a.5">而信者<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-7a.6">未見善事人</seg>
                    <seg xml:id="KR2p0055_tls_001-7a.7" n=".">而憂者</seg>
                    <seg xml:id="KR2p0055_tls_001-7a.8" n="...">今之世<c n="["/>
                        <c n="]"/>
                    </seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.8 (STRIPS 22, 13) MACHENGYUAN 5: 280, 275 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-8a"/>
                    <seg xml:id="KR2p0055_tls_001-8a.1" n="......">
                        <c n="["/>
                        <c n="]"/> 子聞之曰<c n=":"/> <c n="["/>
                        <c n="]"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-8a.2" n=".">
                        <c n="“"/>賜<c n=","/>不吾智知也</seg>
                    <seg xml:id="KR2p0055_tls_001-8a.3">夙興夜寐<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-8a.4" n="...22">以求聞 <c n="["/>
                        <c n="]"/>
                        <c n="【"/>
                        <c n="】"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-8a.5" n="...">
                        <c n="["/>不以 <c n="]"/> 就人<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-8a.6" n=".">不曲防以去人<c n="”"/>
                    </seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.9 (STRIPS 13, 12) MACHENGYUAN 5: 275 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-9a"/>
                    <seg xml:id="KR2p0055_tls_001-9a.1">子曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-9a.2">
                        <c n="“"/>君子亡所不足<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-9a.3">無所有餘<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-9a.4" n="13..">蓋 <c n="【"/>
                        <c n="】"/>
                        <c n="["/>
                        <c n="]"/> 也<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-9a.5" n=".">求為之言</seg>
                    <seg xml:id="KR2p0055_tls_001-9a.6">有夫言也<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-9a.7" n=".">求為之行</seg>
                    <seg xml:id="KR2p0055_tls_001-9a.8">言行相近<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-9a.9" n=".">然後君子<c n="”"/>
                    </seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.10 (STRIPS 12, 15, 16) MACHENGYUAN 5: 275, 276, 277 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-10a"/>
                    <seg xml:id="KR2p0055_tls_001-10a.1" n="12">子<c n="【"/>
                        <c n="】"/>曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-10a.2">
                        <c n="“"/>回<c n="!"/> 來<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-10a.3" n=".">吾告汝</seg>
                    <seg xml:id="KR2p0055_tls_001-10a.4">其阻絕乎<c n="?"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-10a.5">雖多聞</seg>
                    <seg xml:id="KR2p0055_tls_001-10a.6" n="15">而不友賢<c n=","/>其<c n="【"/>
                        <c n="】"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-10a.7" n=".......">
                        <c n="["/>
                        <c n="]"/> 焉終<c n="”"/> <c n="["/>
                        <c n="]"/>
                    </seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.11 (STRIP 16) MACHENGYUAN 5: 277 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-11a"/>
                    <seg xml:id="KR2p0055_tls_001-11a.1">子曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-11a.2">
                        <c n="“"/>寡聞<c n="“"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-11a.3">則孤<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-11a.4">寡見</seg>
                    <seg xml:id="KR2p0055_tls_001-11a.5" n=".">則肆</seg>
                    <seg xml:id="KR2p0055_tls_001-11a.6">多聞</seg>
                    <seg xml:id="KR2p0055_tls_001-11a.7">則惑<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-11a.8" n="...16">多見則<c n="["/>
                        <c n="]"/> <c n="【"/>
                        <c n="】"/>
                    </seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.12 (STRIPS 5, 6) MACHENGYUAN 5: 270, 271 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-12a"/>
                    <seg xml:id="KR2p0055_tls_001-12a.1" n="....">
                        <c n="["/>
                        <c n="]"/> 者<c n=","/>可逆而造也<c n="”"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-12a.2">子曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-12a.3">
                        <c n="“"/>小子來<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-12a.4">取余言<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-12a.5">登年不恆至<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-12a.6">耈老不復壯<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-12a.7" n="5...">賢者忣<c n="【"/>
                        <c n="】"/>
                        <c n="["/>
                        <c n="]"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-12a.8" n="....">
                        <c n="["/>
                        <c n="]"/> 焉<c n="”"/>
                    </seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.13 (STRIPS 6, 9) MACHENGYUAN 5: 271, 273 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-13a"/>
                    <seg xml:id="KR2p0055_tls_001-13a.1">子曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-13a.2">
                        <c n="“"/>貧賤</seg>
                    <seg xml:id="KR2p0055_tls_001-13a.3">而不約者<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-13a.4">吾見之矣<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-13a.5">富貴</seg>
                    <seg xml:id="KR2p0055_tls_001-13a.6">而不驕者<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-13a.7">吾聞</seg>
                    <seg xml:id="KR2p0055_tls_001-13a.8" n=".6">而<c n="["/>未之見也<c n="”"/>
                        <c n="]"/>
                        <c n="【"/>
                        <c n="】"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-13a.9" n="...">
                        <c n="["/>
                        <c n="]"/> 士<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-13a.10">吾見之矣<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-13a.11">事</seg>
                    <seg xml:id="KR2p0055_tls_001-13a.12">而弗受者<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-13a.13">吾聞</seg>
                    <seg xml:id="KR2p0055_tls_001-13a.14" n=".">而未之見也<c n="”"/>
                    </seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.14 (STRIP 9) MACHENGYUAN 5: 273 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-14a"/>
                    <seg xml:id="KR2p0055_tls_001-14a.1">子曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-14a.2">
                        <c n="“"/>人而下臨<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-14a.3" n=".9">猶上臨也<c n="【"/>
                        <c n="】"/>
                    </seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.15 (STRIP 11) MACHENGYUAN 5: 274 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-15a"/>
                    <seg xml:id="KR2p0055_tls_001-15a.1">曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-15a.2">
                        <c n="“"/>巧言令色<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-15a.3" n=".">未可謂仁也</seg>
                    <seg xml:id="KR2p0055_tls_001-15a.4" n=".">
                        <c n="["/>
                        <c n="]"/> 者其言<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-15a.5" n="..">
                        <c n="["/>
                        <c n="]"/>而不可</seg>
                    <seg xml:id="KR2p0055_tls_001-15a.6" n="....">
                        <c n="["/>
                        <c n="]"/> 此之謂仁<c n="”"/>
                    </seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.16 (STRIPS 11, 24) MACHENGYUAN 5: 274, 281 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-16a"/>
                    <seg xml:id="KR2p0055_tls_001-16a.1" n=".">宰我問君子</seg>
                    <seg xml:id="KR2p0055_tls_001-16a.2">曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-16a.3">
                        <c n="“"/>予<c n="!"/> 汝能慎始與終<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-16a.4">斯善矣<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-16a.5" n="11">為君子乎<c n="?"/>
                        <c n="【"/>
                        <c n="】"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-16a.6" n=".24">汝焉能也<c n="”"/>
                        <c n="【"/>
                        <c n="】"/>
                    </seg>
                </p>

<p source="SHANGBO.DIZIWEN 5.26.17 (STRIP 19) MACHENGYUAN 5:278 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0055_tls_001-17a"/>
                    <seg xml:id="KR2p0055_tls_001-17a.1">長<c n=","/>蓬伯玉止乎<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-17a.2">子惇惇如也<c n=";"/>
                    </seg>
                    <seg xml:id="KR2p0055_tls_001-17a.3">聽子路往乎<c n=","/>
                    </seg>
