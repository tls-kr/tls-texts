<p source="SHANGBO.RONGSHIYOUCHENG 5.29.1 (STRIPS 5) MACHENGYUAN 5: 322 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0058_tls_001-1a"/>
                    <seg xml:id="KR2p0058_tls_001-1a.1">融師有成<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0058_tls_001-1a.2">厥狀若猩<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0058_tls_001-1a.3">有耳不聞<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0058_tls_001-1a.4" n=".">有口不鳴</seg>
                    <seg xml:id="KR2p0058_tls_001-1a.5">有目不見<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0058_tls_001-1a.6" n=".">有足不趨</seg>
                    <seg xml:id="KR2p0058_tls_001-1a.7">名則可畏<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0058_tls_001-1a.8">步則可侮</seg>
                </p>

<p source="SHANGBO.RONGSHIYOUCHENG 5.29.2 (STRIPS 5-6) MACHENGYUAN 5: 322, 324 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0058_tls_001-2a"/>
                    <seg xml:id="KR2p0058_tls_001-2a.1">我曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0058_tls_001-2a.2" n="5">虘茖乎<c n="?"/>
                        <c n="?"/>
                        <c n="?"/>
                        <c n="【"/>
                        <c n="】"/>
                    </seg>
                    <seg xml:id="KR2p0058_tls_001-2a.3" n="....">
                        <c n="["/>
                        <c n="]"/> 猷時</seg>
                    <seg xml:id="KR2p0058_tls_001-2a.4">我曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0058_tls_001-2a.5">虘喬乎<c n="?"/>
                        <c n="?"/>
                        <c n="?"/>
                        <c n=","/>
                    </seg>
                    <seg xml:id="KR2p0058_tls_001-2a.6" n=".">弗飲弗食</seg>
                    <seg xml:id="KR2p0058_tls_001-2a.7">物斯可惑<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0058_tls_001-2a.8" n=".">類獸非鼠<c n="("/>
                        <c n="?"/>
                        <c n=")"/>
                    </seg>
                    <seg xml:id="KR2p0058_tls_001-2a.9" n="+.">後<c n="["/>食人弗<c n="]"/>昱</seg>
                    <seg xml:id="KR2p0058_tls_001-2a.10">蔑師</seg>
                    <seg xml:id="KR2p0058_tls_001-2a.11" n=".">見凶<c n="["/>离<c n="]"/>
                    </seg>
                </p>

<p source="SHANGBO.RONGSHIYOUCHENG 5.29.3 (STRIPS 6-7) MACHENGYUAN 5: 324, 326 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0058_tls_001-3a"/>
                    <seg xml:id="KR2p0058_tls_001-3a.1">毀折戮残<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0058_tls_001-3a.2" n=".">唯茲作章</seg>
                    <seg xml:id="KR2p0058_tls_001-3a.3">象彼獸鼠<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0058_tls_001-3a.4" n="6.">有足而<c n="【"/>
                        <c n="】"/>梏</seg>
                    <seg xml:id="KR2p0058_tls_001-3a.5">沈挫念惟<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0058_tls_001-3a.6" n=".">發揚索<c n="["/>縢<c n="]"/>儥</seg>
                </p>

