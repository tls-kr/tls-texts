<text>
<body>
<div>

<p source="SHANGBO.BANGRENBUCHENG 9.58.1 (STRIPS 1-2A) MACHENGYUAN 9: 242-243 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0086_tls_001-1a"/>
                    <seg xml:id="KR2p0086_tls_001-1a.1" n="...">
                        <c n="["/>
                        <c n="]"/> 子乎<c n="?"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-1a.2" n=".">故不以至命</seg>
                    <seg xml:id="KR2p0086_tls_001-1a.3">侵<c n="（"/>箴<c n="?"/>
                        <c n="）"/>尹曰<c n="："/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-1a.4">
                        <c n="“"/>天加訛<c n="（"/>禍<c n="）"/>於楚邦<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-1a.5">吾君盪出<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-1a.6" n="...1">威視 <c n="("/>
                        <c n="?"/>
                        <c n=")"/> <c n="["/>
                        <c n="]"/> <c n="【"/>
                        <c n="】"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-1a.7">亡名焉</seg>
                    <seg xml:id="KR2p0086_tls_001-1a.8" n=".2A">是故弗知也 <c n="【"/>
                        <c n="】"/>
                    </seg>
                </p>

<p source="SHANGBO.BANGRENBUCHENG 9.58.2 (STRIPS 2B-3) MACHENGYUAN 9: 243-245 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0086_tls_001-2a"/>
                    <seg xml:id="KR2p0086_tls_001-2a.1" n="....">
                        <c n="["/>
                        <c n="]"/> 祓天之女</seg>
                    <seg xml:id="KR2p0086_tls_001-2a.2">就昭王之亡<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-2a.3">要 <c n="("/>遇<c n="?"/>
                        <c n="）"/>王於隨待<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-2a.4">戰於澨<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-2a.5">戰於津<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-2a.6" n="2B">戰於長<c n="【"/>
                        <c n="】"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-2a.7" n="...">
                        <c n="["/>
                        <c n="]"/> 曲陶<c n="，"/>
                    </seg>
                </p>

<p source="SHANGBO.BANGRENBUCHENG 9.58.3 (STRIPS 3-4) MACHENGYUAN 9: 245-247 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0086_tls_001-3a"/>
                    <seg xml:id="KR2p0086_tls_001-3a.1">三戰而三止<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-3a.2" n=".">而邦人不稱勇焉</seg>
                    <seg xml:id="KR2p0086_tls_001-3a.3">就復邦之後<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-3a.4">蓋冠爲王獲<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-3a.5" n="3.">而邦人<c n="【"/>
                        <c n="】"/>不稱美焉</seg>
                </p>

<p source="SHANGBO.BANGRENBUCHENG 9.58.4 (STRIP 4) MACHENGYUAN 9: 247 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0086_tls_001-4a"/>
                    <seg xml:id="KR2p0086_tls_001-4a.1">就白公之禍<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-4a.2">聞令尹<c n=","/> 司馬既死<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-4a.3" n=".">將適郢</seg>
                    <seg xml:id="KR2p0086_tls_001-4a.4">葉之諸老皆諫曰<c n="："/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-4a.5">
                        <c n="“"/>不可<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-4a.6" n=".">必以師<c n="”"/>
                    </seg>
                </p>

<p source="SHANGBO.BANGRENBUCHENG 9.58.5 (STRIPS 4-5) MACHENGYUAN 9: 247-250 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0086_tls_001-5a"/>
                    <seg xml:id="KR2p0086_tls_001-5a.1" n="4">葉<c n="【"/>
                        <c n="】"/>公子高曰<c n="："/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-5a.2">
                        <c n="“"/>不得王<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-5a.3">將必死<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-5a.4">何以師爲<c n="?"/>
                        <c n="”"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-5a.5">乃乘馹車五乘<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-5a.6" n=".">遂適郢</seg>
                </p>

<p source="SHANGBO.BANGRENBUCHENG 9.58.6 (STRIPS 5-7) MACHENGYUAN 9: 250-251 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0086_tls_001-6a"/>
                    <seg xml:id="KR2p0086_tls_001-6a.1">至<c n=","/> 未得王<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-6a.2" n="5">昭夫人謂葉公<c n="【"/>
                        <c n="】"/>子高<c n="："/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-6a.3" n="...6">
                        <c n="“"/>先君之子聚在外 <c n="["/>
                        <c n="]"/>
                        <c n="【"/>
                        <c n="】"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-6a.4">君之言過<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-6a.5">昔周<c n="("/>
                        <c n="?"/>
                        <c n=")"/>盍擇而立之<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-6a.6">邦既有王<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-6a.7">毋亦<c n="("/>
                        <c n="?"/>
                        <c n=")"/> 勸乎<c n="?"/>
                    </seg>
                </p>

<p source="SHANGBO.BANGRENBUCHENG 9.58.7 (STRIPS 7-8) MACHENGYUAN 9: 252-253 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0086_tls_001-7a"/>
                    <seg xml:id="KR2p0086_tls_001-7a.1">葉公子高曰<c n="："/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-7a.2">
                        <c n="“"/>万<c n="("/>
                        <c n="?"/>
                        <c n=")"/>千君<c n="["/>㫃旱<c n="（"/>幹<c n="?"/>
                        <c n="）"/>
                        <c n="]"/>
                        <c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-7a.3" n="7">何施獻<c n="【"/>
                        <c n="】"/>之惑也<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-7a.4">而并是二者以邦君<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-7a.5">君猶小之<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-7a.6">一懼君之不終世<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-7a.7" n=".">保邦</seg>
                </p>

<p source="SHANGBO.BANGRENBUCHENG 9.58.8 (STRIPS 8-9) MACHENGYUAN 9: 253-254 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0086_tls_001-8a"/>
                    <seg xml:id="KR2p0086_tls_001-8a.1">既言<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-8a.2" n=".">乃服固祝而止之</seg>
                    <seg xml:id="KR2p0086_tls_001-8a.3" n="8">蔡<c n="【"/>
                        <c n="】"/>大祝止<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-8a.4">須邦君加冠冕爲服<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-8a.5">出就蔡大祝踊<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-8a.6">二拜頓首曰<c n="："/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-8a.7">
                        <c n="“"/>今日踊<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-8a.8">既失邦<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-8a.9" n=".">或得之<c n="”"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-8a.10" n="...9">蔡大 <c n="["/>
                        <c n="]"/>
                        <c n="【"/>
                        <c n="】"/>
                    </seg>
                </p>

<p source="SHANGBO.BANGRENBUCHENG 9.58.9 (STRIPS 11) MACHENGYUAN 9: 257 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0086_tls_001-9a"/>
                    <seg xml:id="KR2p0086_tls_001-9a.1" n=".">寍禍</seg>
                    <seg xml:id="KR2p0086_tls_001-9a.2">賞之以西廣田百畛<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-9a.3">辭曰<c n="："/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-9a.4">
                        <c n="“"/>君王諍臣之請命<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-9a.5" n=".">未嘗不許<c n="”"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-9a.6" n=".">辭不受賞</seg>
                </p>

<p source="SHANGBO.BANGRENBUCHENG 9.58.10 (STRIPS 11, 12, 10) MACHENGYUAN 9: 257-258, 256 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0086_tls_001-10a"/>
                    <seg xml:id="KR2p0086_tls_001-10a.1" n="11">命之為令<c n="【"/>
                        <c n="】"/>尹<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-10a.2" n=".">辭</seg>
                    <seg xml:id="KR2p0086_tls_001-10a.3">命之為司馬<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-10a.4">辭<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-10a.5">曰<c n="："/>saying<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-10a.6">
                        <c n="&#34;"/>以葉之遠<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-10a.7" n=".">不可畜也<c n="&#34;"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-10a.8">焉假為司馬<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-10a.9">不取其實<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0086_tls_001-10a.10" n="12.">而邦人不稱榮<c n="【"/>
                        <c n="】"/>焉</seg>
                </p>

</div>
</body>
