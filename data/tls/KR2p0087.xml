                    <seg xml:id="KR2p0087_tls_001-4a.6" n="...">
                        <c n="["/>
                        <c n="]"/> 必<c n="["/>在<c n="]"/>危其邦家<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0087_tls_001-4a.7">則能貴<c n="["/>潰<c n="]"/>於禹湯<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0087_tls_001-4a.8">禹湯則學<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0087_tls_001-4a.9" n="3">自始<c n="【"/>
                        <c n="】"/>有民以來<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0087_tls_001-4a.10">未或能才立<c n="["/>=栽粒<c n=","/> or =在立<c n="]"/>於地之上<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0087_tls_001-4a.11">一或不免有禍<c n="，"/>
