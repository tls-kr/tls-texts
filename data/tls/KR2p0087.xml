                        <c n="”"/>
                    </seg>
                    <seg xml:id="KR2p0087_tls_001-3a.4">夫子曰<c n="："/>
                    </seg>
                    <seg xml:id="KR2p0087_tls_001-3a.5">
                        <c n="“"/>內邪偽<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0087_tls_001-3a.6">幽色与酒<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0087_tls_001-3a.7" n="6">大鐘鼎<c n="，"/>
