<p source="SHANGBO.SHENGONGCHENLINGWANG 6.33.1 (STRIPS 4B-6) MACHENGYUAN 6: 246-249 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0062_tls_001-1a"/>
                    <seg xml:id="KR2p0062_tls_001-1a.1">圉於朸術<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0062_tls_001-1a.2" n="4B">申公子攀載皇駒 <c n="["/>
                        <c n="]"/>
                    </seg>
                    <seg xml:id="KR2p0062_tls_001-1a.3">王子圍奪之<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0062_tls_001-1a.4" n=".">申公爭之</seg>
                    <seg xml:id="KR2p0062_tls_001-1a.5">王子圍立爲王<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0062_tls_001-1a.6">申公子攀見王<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0062_tls_001-1a.7">王曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0062_tls_001-1a.8" n="5">
                        <c n="“"/>申公<c n="["/>
                        <c n="]"/>忘夫朸術之下乎<c n="?"/>
                        <c n="”"/>
                    </seg>
                </p>

<p source="SHANGBO.SHENGONGCHENLINGWANG 6.33.2 (STRIPS 6-8) MACHENGYUAN 6: 249-250 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0062_tls_001-2a"/>
                    <seg xml:id="KR2p0062_tls_001-2a.1">申公曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0062_tls_001-2a.2" n=".">
                        <c n="“"/>臣不知君王之將爲君</seg>
                    <seg xml:id="KR2p0062_tls_001-2a.3" n="6">如臣知君王 <c n="["/>
                        <c n="]"/> 之爲君<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0062_tls_001-2a.4" n=".">臣將或致焉<c n="”"/>
                    </seg>
                    <seg xml:id="KR2p0062_tls_001-2a.5">王曰<c n=":"/>
                    </seg>
                    <seg xml:id="KR2p0062_tls_001-2a.6">
                        <c n="“"/>不穀以笑申公<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0062_tls_001-2a.7" n=".">是言棄之</seg>
                    <seg xml:id="KR2p0062_tls_001-2a.8" n="7">今日<c n="["/>
                        <c n="]"/>申公事不穀<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0062_tls_001-2a.9" n=".">必以是心<c n="”"/>
                    </seg>
                </p>

