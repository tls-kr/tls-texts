<p source="SHANGBO.ZIDAOE 8.44.1 (STRIPS 4-5) MACHENGYUAN 8: 127-128 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0073_tls_001-1a"/>
                    <seg xml:id="KR2p0073_tls_001-1a.1">魯司寇寄言遊於逡楚<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0073_tls_001-1a.2">曰<c n="："/>
                    </seg>
                    <seg xml:id="KR2p0073_tls_001-1a.3">
                        <c n="“"/>除乎<c n="！"/> <c n="“"/>
                    </seg>
                    <seg xml:id="KR2p0073_tls_001-1a.4" n="4.">司寇<c n="【"/>
                        <c n="】"/>𨟻將見我<c n="”"/>
                    </seg>
                    <seg xml:id="KR2p0073_tls_001-1a.5">門人旣除<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0073_tls_001-1a.6" n=".">而司寇不至</seg>
                    <seg xml:id="KR2p0073_tls_001-1a.7" n=".">𧧑言遊去</seg>
                </p>

<p source="SHANGBO.ZIDAOE 8.44.2 (STRIPS 5,3,2) MACHENGYUAN 8: 128, 126, 124 TR. MARCO CABOARA">
                    <pb xml:id="KR2p0073_tls_001-2a"/>
                    <seg xml:id="KR2p0073_tls_001-2a.1" n="5...">司<c n="【"/>
                        <c n="】"/>
                        <c n="["/>寇<c n="]"/> <c n="["/>
                        <c n="]"/>
                    </seg>
                    <seg xml:id="KR2p0073_tls_001-2a.2">
                        <c n="“"/>𨟻將焉往<c n="?"/>
                        <c n="”"/>
                    </seg>
                    <seg xml:id="KR2p0073_tls_001-2a.3">𧧑言遊曰<c n="："/>
                    </seg>
                    <seg xml:id="KR2p0073_tls_001-2a.4">
                        <c n="“"/>食而弗與爲禮<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0073_tls_001-2a.5" n="3.">是<c n="("/>戰攻<c n=")"/>
                        <c n="["/>商工<c n="]"/>畜<c n="【"/>
                        <c n="】"/>之也 </seg>
                    <seg xml:id="KR2p0073_tls_001-2a.6">偃 也修其德行<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0073_tls_001-2a.7">以受<c n="("/>戰攻<c n=")"/>
                        <c n="["/>商工<c n="]"/>之食於子<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0073_tls_001-2a.8">於偃僞<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0073_tls_001-2a.9">於子損<c n="，"/>
                    </seg>
                    <seg xml:id="KR2p0073_tls_001-2a.10">於是乎何待<c n="?"/>
                        <c n="”"/>
                    </seg>
                </p>

</div>
