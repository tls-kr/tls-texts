                    <pb xml:id="KR2p0080_tls_001-3a"/>
                    <seg xml:id="KR2p0080_tls_001-3a.1" n=".">離居而同欲今兮</seg>
                    <seg xml:id="KR2p0080_tls_001-3a.2">周流天下今兮<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0080_tls_001-3a.3" n=".">將莫惶今兮</seg>
                    <seg xml:id="KR2p0080_tls_001-3a.4">有不善心耳今兮<c n=","/>
                    </seg>
                    <seg xml:id="KR2p0080_tls_001-3a.5" n=".">莫不變改今兮</seg>
